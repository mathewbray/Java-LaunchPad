	hKTIBDKPPKEdition                PPKEdition,
	hKTIBDKPPKSegment                HaipeSegment,
	hKTIBDKRowStatus                 RowStatus
    }

hKTIBDKShortTitle OBJECT-TYPE
    SYNTAX      PPKShortTitle
    MAX-ACCESS  read-create
    STATUS      current
    DESCRIPTION
        "(U//FOUO) The short title of the disabled key."
