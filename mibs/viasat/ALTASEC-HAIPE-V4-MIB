        altasecHv4GDCSolicitDelivRangeEnd        IPAddress,
        altasecHv4GDCSolicitDelivStatus          OCTET STRING
    }
    
altasecHv4GDCSolicitDelivRangeStart         OBJECT-TYPE
    SYNTAX                    IPAddress
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO) The starting IPv4 or IPv6 address for the range that
        will cause the device to initiate Dynamic Discovery."
        
    ::= { altasecHv4GDCSolicitDelivEntry 2 }
    
altasecHv4GDCSolicitDelivRangeEnd         OBJECT-TYPE
    SYNTAX                    IPAddress
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO) The end IPv4 or IPv6 address for the range that
        will cause the device to initiate Dynamic Discovery."
        
    ::= { altasecHv4GDCSolicitDelivEntry 3 }

altasecHv4GDCSolicitDelivStatus         OBJECT-TYPE
    SYNTAX                    OCTET STRING
    MAX-ACCESS                read-only
    STATUS                    current
    DESCRIPTION
        "(U//FOUO) The current status of the solicitation delivery entry."
        
    ::= { altasecHv4GDCSolicitDelivEntry 4 }
    
