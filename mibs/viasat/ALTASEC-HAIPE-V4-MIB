altasecHv4BatteryTableCount  OBJECT-TYPE
        SYNTAX               Unsigned32
        MAX-ACCESS           read-only
        STATUS               current
        DESCRIPTION
            "(U//FOUO) The number of rows in the altasecHv4BatteryTable."
        ::= { altasecHv4MgmtBattery 1 }

altasecHv4BatteryTableLastChanged     OBJECT-TYPE
        SYNTAX                        TimeStamp
        MAX-ACCESS                    read-only
        STATUS                        current
        DESCRIPTION
            "(U//FOUO) The value of hSystemUpTime the last time any entry in
             the altasecHv4BatteryTable was modified, created, or deleted
             by either SNMP or other management method (e.g., via an HMI).

             (U//FOUO) Managers can use this object to ensure that no
             changes to configuration of this table have happened since
             the last time it examined the table.

             (U//FOUO) A value of 0 indicates that no entry has been
             changed since the agent initialized."
        ::= { altasecHv4MgmtBattery 2 }
        
altasecHv4BatteryTable        OBJECT-TYPE
    SYNTAX                    SEQUENCE OF AltasecHv4BatteryEntry
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) The table describing the status of the batteries in
		the HAIPE."
    ::= { altasecHv4MgmtBattery 3 }

altasecHv4BatteryEntry   OBJECT-TYPE
    SYNTAX                    AltasecHv4BatteryEntry
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) A row describing a particular battery level
        indication within the HAIPE"
    INDEX  { altasecHv4BatteryNumber }
    ::= { altasecHv4BatteryTable 1 }

AltasecHv4BatteryEntry ::= SEQUENCE
    {
        altasecHv4BatteryNumber            Unsigned32,
        altasecHv4BatteryOperStatus        INTEGER,
        altasecHv4BatteryChangeDate        DateAndTime
    }
    
altasecHv4BatteryNumber    OBJECT-TYPE
    SYNTAX                 Unsigned32
    MAX-ACCESS             not-accessible
    STATUS                 current
    DESCRIPTION
        "(U//FOUO) The unique number enumerating a given battery."
    ::= { altasecHv4BatteryEntry 1 }

altasecHv4BatteryOperStatus      OBJECT-TYPE
    SYNTAX                       INTEGER { nominal(1), low(2) }
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION
        "(U//FOUO) This object indicates the current state of a given
        battery. If hBatteryLevelWarningPercentage is a supported
        object then this object will be low when
        hBatteryLevelPercentage <= hBatteryLevelWarningPercentage."
    ::= { altasecHv4BatteryEntry 2 }

altasecHv4BatteryChangeDate     OBJECT-TYPE
    SYNTAX                      DateAndTime
    MAX-ACCESS                  read-write
    STATUS                      current
    DESCRIPTION
        "(U//FOUO) The date and time a changeable battery was
        last replaced. If a battery is changeable, but has not 
        been changed or the last change time was lost then this 
        value should be composed with all 0 values."
    ::= { altasecHv4BatteryEntry 3 }

