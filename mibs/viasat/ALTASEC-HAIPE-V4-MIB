-- *************************************************
-- Registration Discovery.
--
        
altasecHv4GDCRegistrationTableCount  OBJECT-TYPE
        SYNTAX              Unsigned32
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION
            "(U//FOUO) The number of rows in the altasecHv4GDCRegistrationTable."
        ::= { altasecHv4GDCRegistration 1 }

altasecHv4GDCRegistrationTableLastChanged  OBJECT-TYPE
        SYNTAX                    TimeStamp
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
            "(U//FOUO) The value of hSystemUpTime the last time any entry in
             the altasecHv4GDCRegistrationTable was modified, created, or deleted
             by either SNMP or other management method (e.g., via an HMI).

             (U//FOUO) Managers can use this object to ensure that no
             changes to configuration of this table have happened since
             the last time it examined the table.

             (U//FOUO) A value of 0 indicates that no entry has been
             changed since the agent initialized."
        ::= { altasecHv4GDCRegistration 2 }

altasecHv4GDCRegistrationDefaultLifetime OBJECT-TYPE
        SYNTAX      Unsigned32
        UNITS       "seconds"
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "(U//FOUO) This value indicates the default 'prefix
            lifetime parameter' to use when registering an address
            range with a discovery server.  This value should be used
            in registration messages when no other more specific
            information is available."
        DEFVAL { 4294967295 }
        ::= { altasecHv4GDCRegistration 3 }
        
altasecHv4GDCRegistrationTable         OBJECT-TYPE
    SYNTAX                    SEQUENCE OF AltasecHv4GDCRegistrationEntry
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION
        "(U//FOUO) The table containing proprietary information about 
        a set of PT IP addresses (multicast or unicast) that discovery 
        REGISTRATION messages should be sent to.

        (U//FOUO) REGISTRATION messages will be sent to all addresses specified
        in this table.

        (U//FOUO) If the PT address of a given discovery registration
        destination is remote, the REGISTRATION messages to that server will 
        be treated as an outbound PT packet.  Hence, this message will need
        verification against the hPolicyRuleTable to determine the appropriate
        level of protection needed.

        (U//FOUO) Note: The HAIPE IS refers to this as the 'Registration
        Transmission Address Table'."
    ::= { altasecHv4GDCRegistration 4 }

altasecHv4GDCRegistrationEntry         OBJECT-TYPE
    SYNTAX                    AltasecHv4GDCRegistrationEntry
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) A row describing the information about a particular 
         PT IP address that discovery REGISTRATION messages should be sent to."
        
        AUGMENTS   { hGDCRegistrationTableEntry }
    ::= { altasecHv4GDCRegistrationTable 1 }

AltasecHv4GDCRegistrationEntry ::= SEQUENCE
    {
        altasecHv4GDCRegistrationPriority        Unsigned32,
        altasecHv4GDCRegistrationStatus          AltasecHv4DiscoveryStatus
    }
    
altasecHv4GDCRegistrationPriority         OBJECT-TYPE
    SYNTAX                    Unsigned32
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO) A unique numeric index for each configured 
        Registration Address.

        (U//FOUO) Note: Despite the name, this value does not imply 
        a fixed ordering or prioritization of Registration Addresses."
        
    ::= { altasecHv4GDCRegistrationEntry 4 }

altasecHv4GDCRegistrationStatus         OBJECT-TYPE
    SYNTAX                    AltasecHv4DiscoveryStatus
    MAX-ACCESS                read-only
    STATUS                    current
    DESCRIPTION
        "(U//FOUO) The current status of the Registration entry. The status
        may be active or inactive."
        
    ::= { altasecHv4GDCRegistrationEntry 5 }

