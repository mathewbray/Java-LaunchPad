-- This entire document, which defines ViaSat Proprietary MIB objects in the
-- SMIv2 language should be considered U//FOUO,PR.
--
-- The data that will be transmitted on the wire as a result of
-- accessing these MIB objects MUST NOT BE CONSIDERED U//FOUO and is
-- subject to separate classification.  The classification level of this
-- data is not defined or within the scope of this document.
--

ALTASEC-HAIPE-V4-MIB DEFINITIONS ::= BEGIN

IMPORTS
    OBJECT-TYPE, NOTIFICATION-TYPE,
    Integer32, Unsigned32,
    Counter64, MODULE-IDENTITY            FROM SNMPv2-SMI
    TEXTUAL-CONVENTION,
    RowStatus, DateAndTime, TruthValue,
    TimeStamp, DisplayString              FROM SNMPv2-TC
    IPAddressType,
    IPAddress,
    HaipeIKEv1SATransformSuites,
    HaipeDeviceSide,
    PreplacedKeyEdition,
    PreplacedKeySegment,
    DGSharedKeyShortTitle,
    PreplacedKeyShortTitle				  FROM HAIPE-V4-TEXTUAL-CONVENTIONS-MIB 
    SnmpAdminString                       FROM SNMP-FRAMEWORK-MIB
    hIKEv2CHILDSADTableEntry,
    hIKEv1NtwkParamsProfileTableEntry,
    hIKEv1SADTableEntry,
    hIKEv2IKESADTableEntry,
    hManualSADTableEntry                  FROM HAIPE-V4-TRAFFIC-PROTECTION-MIB
    hLEPTTableEntry,
    hPEPTTableEntry,
    hGDCSolicitDelivTableEntry,
    hLegacyDeliveryTableEntry,
    hRIPTableEntry,
    hGDCRegistrationTableEntry            FROM HAIPE-V4-DISCOVERY-MIB
    usmUserSecurityName                   FROM SNMP-USER-BASED-SM-MIB
    hIPv4IfaceTableEntry,
    hIPv6IfaceTableEntry,                  
    hAddressAssignTableEntry              FROM HAIPE-V4-NETWORKING-MIB
    hOOBKTClientsTableEntry,
    hIBKTClientsTableEntry,
    hLoadedKeyTableEntry                  FROM HAIPE-V4-KEY-TRANSFER-MIB
    hDGSharedKeyTableEntry,
    hAPPKTATableEntry,
    hManSAKeyChainTableEntry	            FROM HAIPE-V4-KEY-INFORMATION-MIB
    hFwDwnldServerTableEntry              FROM HAIPE-V4-MANAGEMENT-MIB
    altasecMIB                            FROM VIASAT-PRODUCT-MIB;


altasecHv4Mib MODULE-IDENTITY
    LAST-UPDATED "201506020000Z"
    ORGANIZATION "ViaSat"
    CONTACT-INFO
         "(U//FOUO) Not Available"
    DESCRIPTION
        "(U//FOUO) This MIB defines the ViaSat Proprietary MIB objects for HAIPEv4."
    REVISION "201506020000Z"
    DESCRIPTION "(U//FOUO//PROPIN) ViaSat Propritary MIB 2.0"
    REVISION "201112100000Z"
    DESCRIPTION "(U//FOUO//PROPIN) ViaSat Propritary MIB 1.0"
    ::= {  altasecMIB 100 }


