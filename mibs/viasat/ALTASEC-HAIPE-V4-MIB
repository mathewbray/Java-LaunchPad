-- *************************************************
-- PPK Key Information.
--

altasecHv4PreplacedKeyTableCount OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) The number of rows in the altasecPreplacedKeyTable."
        ::= { altasecHv4PPKKeyInformation 1 }

altasecHv4PreplacedKeyTableLastChanged OBJECT-TYPE
        SYNTAX      	TimeStamp
        MAX-ACCESS  	read-only
        STATUS      	current
        DESCRIPTION
            "(U//FOUO,PR) The value of sysUpTime the last time any entry
            in the altasecPreplacedKeyTable was modified, created, or
            deleted by either SNMP or other management method
            (e.g. via an HMI).

            (U//FOUO,PR) Managers can use this object to ensure that no
            changes to configuration of this table have happened
            since the last time it examined the table.

            (U//FOUO,PR) A value of 0 indicates that no entry has been
            changed since the agent initialized."
        ::= { altasecHv4PPKKeyInformation 2 }

altasecHv4PreplacedKeyTable  OBJECT-TYPE
    SYNTAX            	     SEQUENCE OF AltasecHv4PreplacedKeyEntry
    MAX-ACCESS               not-accessible
    STATUS                   current
    DESCRIPTION
        "(U//FOUO,PR) The table containing information about the loaded
         pre-placed keys (PPK's). This proprietary table is necessary in that
         users may optionally load multiple copies of the same key. This table
         differs from the hPreplacedKeyTable in that the PPK SPIDs are used as
         an additional index into the table. In addition to the values provided
         in the hPreplacedKeyTable, the PPK Chain to which it is assigned (if
         any) is provided for added granularity."
    ::= { altasecHv4PPKKeyInformation 3 }

altasecHv4PreplacedKeyEntry  OBJECT-TYPE
    SYNTAX                AltasecHv4PreplacedKeyEntry
    MAX-ACCESS            not-accessible
    STATUS                current
    DESCRIPTION
        "(U//FOUO,PR) A row indicating a pre-placed key (PPK) within a
        chain of PPK's and the order that the PPK indicated will be
        used by the HAIPE device as part of that chain."
    INDEX  { hV4PPKShortTitleName, hV4PPKEdition, hV4PPKSegment, 
    altasecV4PPKSPID }
    ::= { altasecHv4PreplacedKeyTable 1 }

AltasecHv4PreplacedKeyEntry ::= SEQUENCE
    {
        hV4PPKShortTitleName               PreplacedKeyShortTitle,
        hV4PPKEdition                      PreplacedKeyEdition,
        hV4PPKSegment                      PreplacedKeySegment,
        altasecV4PPKSPID                   Unsigned32,
        hV4PPKisAuthenticated              TruthValue,
        hV4PPKIBKTSupersessionDisable      TruthValue,
        hV4PPKUsageType                    BITS,
        hV4PPKSecurityLevel                BITS,
        hV4PPKLabel                        SnmpAdminString,
        hV4PPKRowStatus                    RowStatus,
        hV4PPKEffectiveDate                DateAndTime,
        altasecHv4PPKChainID               SnmpAdminString,
        altasecHv4PPKIKEVersion            BITS,
        altasecHv4PPKEndDate               DateAndTime,
        altasecHv4PPKQKeyLabel             SnmpAdminString
    }

hV4PPKShortTitleName    OBJECT-TYPE
        SYNTAX          PreplacedKeyShortTitle
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The short title of the pre-placed key (PPK)."
        ::= { altasecHv4PreplacedKeyEntry 1 }

hV4PPKEdition           OBJECT-TYPE
        SYNTAX          PreplacedKeyEdition
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The edition of this pre-placed key (PPK)."
        ::= { altasecHv4PreplacedKeyEntry 2 }

hV4PPKSegment           OBJECT-TYPE
        SYNTAX          PreplacedKeySegment
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The segment number of this pre-placed key (PPK)."
        ::= { altasecHv4PreplacedKeyEntry 3 }

altasecV4PPKSPID        OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) The SPID assigned to the pre-placed key (PPK)."
        ::= { altasecHv4PreplacedKeyEntry 4 }
        
hV4PPKisAuthenticated   OBJECT-TYPE
		SYNTAX          TruthValue
		MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION
		    "(U//FOUO) Indicates whether the key is an authenticated 
		   	pre-placed key (APPK). This column is provided for 
		   	informational purposes, specifically to indicate whether 
		   	this key is Suite A or Suite B. This information is taken 
		   	directly from the key material itself."
        ::= { altasecHv4PreplacedKeyEntry 5 }
        
hV4PPKIBKTSupersessionDisable   OBJECT-TYPE
		SYNTAX                  TruthValue
		MAX-ACCESS              read-write
		STATUS                  current
		DESCRIPTION
		    "(U//FOUO) Indicates whether supersession is allowed for 
		    this pre-placed key (PPK) via In-Band Key Transfer.

            (U//FOUO) [true] If this column is set to true, IBKT 
            supersession is disabled for this key.

            (U//FOUO) [false] If this column is set to false, IBKT 
            supersession is allowed for this key."
        ::= { altasecHv4PreplacedKeyEntry 6 }

hV4PPKUsageType         OBJECT-TYPE
        SYNTAX          BITS { manual-sa(0), ek(1), shared-secret(2) }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) Indicates how this key can be used.  Unlike the
            hPreplacedKeyTable, this value is read-only."
        ::= { altasecHv4PreplacedKeyEntry 7 }

hV4PPKSecurityLevel     OBJECT-TYPE
        SYNTAX          BITS { unclassified(0), restricted(1), confidential(2),
        					   secret(3), top-secret(4), nato-restricted(5), 
        					   nato-confidential(6), nato-secret (7), 
        					   cosmic-top-secret(8) }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The security level usable with this
            pre-placed key (PPK)."
        ::= { altasecHv4PreplacedKeyEntry 8 }
        
hV4PPKLabel				OBJECT-TYPE
		SYNTAX			SnmpAdminString	
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION
			"(U//FOUO) This object is a free text field allowing the user
			to tag PPKs with helpful information. For example, a label
			could indicate the intended usage of a particular PPK."
		::= { altasecHv4PreplacedKeyEntry 9 }
        
hV4PPKRowStatus         OBJECT-TYPE
        SYNTAX          RowStatus
        MAX-ACCESS      read-create
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The status of the row by which new entries may be
			created, existing entries enabled/disabled or existing entries
			deleted from this table.

			(U//FOUO) Entries created within this table may not become active
			unless all columns of this table have valid values. Entries may
			be active, however, even though they are currently not used to
			protect traffic. For example, a chain for future use may have an
			effective start date beyond the current time.

			(U//FOUO) At a minimum, implementations must support createAndGo
			and destroy management functions. Support for createAndWait,
			active, notInService, and notReady management functions is
			optional."
        ::= { altasecHv4PreplacedKeyEntry 10 } 

hV4PPKEffectiveDate     OBJECT-TYPE
        SYNTAX          DateAndTime
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The effective start date and time of this
            pre-placed key (PPK).  This object will be automatically
            set from the chain information once the spot in the chain
            is reached where this key becomes actively used."
        ::= { altasecHv4PreplacedKeyEntry 11 }

altasecHv4PPKChainID   OBJECT-TYPE
        SYNTAX      SnmpAdminString
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "(U//FOUO,PR) An administratively assigned name to link a group
            of PPKs together.  This object may be referenced by the
            hIPhRuleActionIndex object in the hIPhRuleTable if it is not a
            zero-length string."
        ::= { altasecHv4PreplacedKeyEntry 12 }

        
altasecHv4PPKIKEVersion     OBJECT-TYPE
        SYNTAX        BITS { ikev1(0), ikev2(1) }
        MAX-ACCESS    read-write
        STATUS        current
        DESCRIPTION
            "(U//FOUO,PR) The version of IKE to use when assigning the key.
            Note that IKEv2 is only available if it has been enabled on the
            device."
        ::= { altasecHv4PreplacedKeyEntry 13 }
        
altasecHv4PPKEndDate     OBJECT-TYPE
        SYNTAX          DateAndTime
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The effective end date and time of this
            pre-placed key (PPK).  This object will be automatically
            set from the chain information once the spot in the chain
            is reached where this key becomes actively used. This column only
            exists if the key is assigned as a Shared Secret."
        ::= { altasecHv4PreplacedKeyEntry 14 }
        
altasecHv4PPKQKeyLabel   OBJECT-TYPE
        SYNTAX        SnmpAdminString
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION
            "(U//FOUO,PR) An administratively assigned name to identify
            the Q-key (trust anchor) used to authenticate an APPK.
            This object only exists when the INE is operating in Suite B,
            and Q-Key is enabled."
        ::= { altasecHv4PreplacedKeyEntry 15 }

        
-- *************************************************
-- PPK Chain Information.
