-- *************************************************
-- Remote Management.

-- added old syntax values to description (CHIMERA Team)
altasecHv4MgmtRemoteSetting OBJECT-TYPE
        SYNTAX              INTEGER 
                            { 
                               disabled(0), 
                               everyone(1), 
                               ipTable(2) }
        MAX-ACCESS          read-write
        STATUS              current
        DESCRIPTION
            "(U//FOUO,PR) The setting for how remote management shoudl be
             handled. Disabled indicates that remote management is disabled.
             everyone indicates that everyone is able to remotely manage
             this device. ipTable indicates that the 
             altsecHv4MgmtRemoteAddressTable is used to filter who is remotely
             able to manage this device.

             (U//FOUO,PR) Old values: enabled(0), disabled(1), single(2)."
        ::= { altasecHv4MgmtRemote 1 }

altasecHv4MgmtRemoteAddress4 OBJECT-TYPE
        SYNTAX               IPAddress
        MAX-ACCESS           read-write
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) Remote Management IPv4 address."
        ::= { altasecHv4MgmtRemote 2 }

altasecHv4MgmtRemoteAddress6 OBJECT-TYPE
        SYNTAX               IPAddress
        MAX-ACCESS           read-write
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) Remote Management IPv6 address."
        ::= { altasecHv4MgmtRemote 3 }
        
altasecHv4MgmtUDPZeroizeInterval  OBJECT-TYPE
        SYNTAX                    Unsigned32(10..600)
        MAX-ACCESS                read-write
        STATUS                    current
        DESCRIPTION
            "(U//FOUO,PR) UDP Management interval before a zeroize executes."
        DEFVAL { 50 }
        ::= { altasecHv4MgmtRemote 4 }

altasecHv4MgmtUDPZeroize OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) UDP Management zeroize flag."
        ::= { altasecHv4MgmtRemote 5 }

altasecHv4MgmtUDPPort    OBJECT-TYPE
        SYNTAX           Unsigned32(0..65535)
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) UDP Management IP port. Setting this value to
            0 disables UDP Management."
        DEFVAL { 3000 }
        ::= { altasecHv4MgmtRemote 6 }
        
altasecHv4MgmtUDPRememberHbt    OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) UDP Management Remember Heartbeat flag. Setting this
            flag causes the INE to remember if a Heartbeat has been previously
            received across reboots."
        ::= { altasecHv4MgmtRemote 7 }
        
-- *************************************************
-- Remote Management Address Table.

altasecHv4MgmtRemoteAddressTableCount       OBJECT-TYPE
        SYNTAX              Unsigned32
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION
            "(U//FOUO,PR) The number of rows in the altasecHv4MgmtRemoteAddressTable."
        ::= { altasecHv4MgmtRemote 8 }

altasecHv4MgmtRemoteAddressTableTableLastChanged OBJECT-TYPE
        SYNTAX              TimeStamp
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION
            "(U//FOUO,PR) The value of sysUpTime the last time any entry in
             the altasecHv4MgmtRemoteAddressTable was modified, created, or deleted by
             either SNMP or other management method (e.g., via an HMI).

             (U//FOUO,PR) Managers can use this object to ensure that no
             changes to configuration of this table have happened since the
             last time it examined the table.

             (U//FOUO,PR) A value of 0 indicates that no entry has been
             changed since the agent initialized."
        ::= { altasecHv4MgmtRemote 9 }

altasecHv4MgmtRemoteAddressTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF AltasecHv4MgmtRemoteAddressEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) A table containing a list of networks that are 
             allowed to remotely manage this HAIPE. Note that networks 
             must be distinct (i.e. An Entry within this table cannot be 
             a sub net of another entry)."
        ::= { altasecHv4MgmtRemote 10 }

altasecHv4MgmtRemoteAddressEntry OBJECT-TYPE
        SYNTAX          AltasecHv4MgmtRemoteAddressEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) A row defining the IP configuration for a remote management
             network."
        INDEX   { altasecHv4MgmtRemoteAddressType,
                  altasecHv4MgmtRemoteAddressStart }
        ::= { altasecHv4MgmtRemoteAddressTable 1 }

AltasecHv4MgmtRemoteAddressEntry ::= SEQUENCE
    {
        altasecHv4MgmtRemoteAddressType            IPAddressType,
        altasecHv4MgmtRemoteAddressStart           IPAddress,
        altasecHv4MgmtRemoteAddressPrefixLength    Unsigned32,
        altasecHv4MgmtRemoteAddressLabel           OCTET STRING,
        altasecHv4MgmtRemoteAddressRowStatus       RowStatus
    }

altasecHv4MgmtRemoteAddressType     OBJECT-TYPE
