-- RIP Discovery.
--
        
altasecHv4RIPTableCount     OBJECT-TYPE
        SYNTAX              Unsigned32
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION
            "(U//FOUO) The number of rows in the altasecHv4RIPTable."
        ::= { altasecHv4RIPConfig 1 }

altasecHv4RIPTableLastChanged  OBJECT-TYPE
        SYNTAX                    TimeStamp
        MAX-ACCESS                read-only
        STATUS                    current
        DESCRIPTION
            "(U//FOUO) The value of hSystemUpTime the last time any entry in
             the altasecHv4RIPTable was modified, created, or deleted
             by either SNMP or other management method (e.g., via an HMI).

             (U//FOUO) Managers can use this object to ensure that no
             changes to configuration of this table have happened since
             the last time it examined the table.

             (U//FOUO) A value of 0 indicates that no entry has been
             changed since the agent initialized."
        ::= { altasecHv4RIPConfig 2 }
    
altasecHv4RIPTable         	OBJECT-TYPE
    SYNTAX                    SEQUENCE OF AltasecHv4RIPEntry
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION
        "(U//FOUO) This table is an extension of the hRIPTable."
    ::= { altasecHv4RIPConfig 3 }

altasecHv4RIPEntry       OBJECT-TYPE
    SYNTAX                    AltasecHv4RIPEntry
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) A row describing the information about a particular 
         RIP entry."
        
        AUGMENTS   { hRIPTableEntry }
    ::= { altasecHv4RIPTable 1 }

AltasecHv4RIPEntry ::= SEQUENCE
    {
    	altasecHv4RIPExpirationTimer        Unsigned32,
        altasecHv4RIPFlushTimer             Unsigned32,
        altasecHv4RIPUpdateTimer            Unsigned32,
        altasecHv4RIPReceive                INTEGER
    }
    
altasecHv4RIPExpirationTimer  OBJECT-TYPE
    SYNTAX                    Unsigned32 (5..999)
    UNITS                     "seconds"
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) The RIP expiration timer, in seconds."
        
    ::= { altasecHv4RIPEntry 3 }
    
altasecHv4RIPFlushTimer       OBJECT-TYPE
    SYNTAX                    Unsigned32 (5..999)
    UNITS                     "seconds"
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) The RIP flush timer, in seconds."
        
    ::= { altasecHv4RIPEntry 4 }
    
altasecHv4RIPUpdateTimer      OBJECT-TYPE
    SYNTAX                    Unsigned32 (5..999)
    UNITS                     "seconds"
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) The RIP update timer, in seconds."
        
    ::= { altasecHv4RIPEntry 5 }
    
altasecHv4RIPReceive OBJECT-TYPE
    SYNTAX   INTEGER {
                    doNotReceive (1),
                    rip1 (2),
                    rip2 (3),
                    rip1OrRip2 (4),
                    ripNG (5)
                 }
    MAX-ACCESS	 read-write
    STATUS		 current
    DESCRIPTION
        "(U//FOUO,PR) The version(s) of RIP updates that are to be accepted.  
        Note that rip2 and rip1OrRip2 implies reception of multicast
        packets.
        
        (U//FOUO,PR) This column extends the haipe version to identify the 
        various types."
    DEFVAL { doNotReceive }
    ::= { altasecHv4RIPEntry 6 }
    
END

