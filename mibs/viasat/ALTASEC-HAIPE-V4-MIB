altasecHv4PEPTTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF AltasecHv4PEPTEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
        "(U//FOUO) ViaSat table augmenting the table containing a list of the
         known Remote PT address hosts and/or prefixes that this device is
         fronting.  The contents of this table can be populated by either
         manual assignment or through the use of discovery services (e.g.,
         RIP). It is used when routing post-decryption inbound packets to the
         PT enclave."
    ::= { altasecHv4PEPT 3 }

altasecHv4PEPTEntry OBJECT-TYPE
    SYNTAX     AltasecHv4PEPTEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
            "(U//FOUO,PR) A row describing the additional proprietary
             information about current Remote Routes that is not included
             in the HAIPE hPEPTTable."
    AUGMENTS { hPEPTTableEntry }
    ::= { altasecHv4PEPTTable 1 }

AltasecHv4PEPTEntry ::= SEQUENCE
{
   altasecHv4PEPTMetric                            INTEGER,
   altasecHv4PEPTLabel                             OCTET STRING,
   altasecHv4PEPTConnect                           TruthValue,
   altasecHv4PEPTTunnelAssociationName             SnmpAdminString
}

altasecHv4PEPTMetric OBJECT-TYPE
    SYNTAX     INTEGER
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "(U//FOUO) The primary routing metric for this route.  The semantics of
         this metric are determined by the routing protocol specified in the
         route's CIDR Route Table Route Protocol value. Warning: Changing the
         metric of an SA used by automated SAs could cause re-negotiation."
    ::= { altasecHv4PEPTEntry 6 }

altasecHv4PEPTLabel        OBJECT-TYPE
        SYNTAX             OCTET STRING
        MAX-ACCESS         read-write
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) Label associated with remote route."
        ::= { altasecHv4PEPTEntry 7 }

altasecHv4PEPTConnect      OBJECT-TYPE
        SYNTAX             TruthValue
        MAX-ACCESS         read-write
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) Set to true to attempt to connect remote route, false
             otherwise."
    ::= { altasecHv4PEPTEntry 8 }

altasecHv4PEPTTunnelAssociationName OBJECT-TYPE
        SYNTAX                      SnmpAdminString
        MAX-ACCESS                  read-write
        STATUS                      current
        DESCRIPTION
            "(U//FOUO,PR) The device name of the PPK Tunnel SA that is 
             currently associated with this entry. If the PEPT entry does not 
             have a Tunnel association, a zero length string is returned."
    ::= { altasecHv4PEPTEntry 9 }
    
