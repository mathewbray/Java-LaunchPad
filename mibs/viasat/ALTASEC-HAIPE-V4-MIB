-- =======================
-- Networking Scalars
-- =======================

altasecHv4IPMulticastEnable OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) Global multicast enable Truth Value."
        ::= { altasecHv4NetScalars 1 }

altasecHv4IPReplayWindowSize OBJECT-TYPE
        SYNTAX            Unsigned32(32..32768)
        MAX-ACCESS        read-write
        STATUS            current
        DESCRIPTION
            "(U//FOUO,PR) The window size, in packets, that are used for
             anti-replay integrity, which detects arrival of duplicate IP
             datagrams (within a constrained window). In anti-replay integrity a
             packet's unique sequence number is compared to that of the
             previous packets (whose number to examine is defined by this value)
             on arrival.

             (U//FOUO,PR) This value may be 32, 64, 256, 1024, 4096, 8192,
             16384, or 32768. Note that in this progression 128, 512, and 2048
             are not supported."
        ::= { altasecHv4NetScalars 2 }

altasecHv4IPMulticastIGMPQueryEnable OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) IGMP query multicast enable Truth Value."
        ::= { altasecHv4NetScalars 3 }

altasecHv4IPMulticastIGMPVersion OBJECT-TYPE
        SYNTAX           INTEGER { none(0),
                                   version1(1),
                                   version2(2),
                                   version3(3)
                                 }
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) IGMP multicast version."
        ::= { altasecHv4NetScalars 4 }

altasecHv4IPAODVEnable OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) AODV enable Truth Value."
        ::= { altasecHv4NetScalars 5 }

altasecHv4IPMulticastMLDQueryEnable OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) MLD Query multicast enable Truth Value."
        ::= { altasecHv4NetScalars 6 }

altasecHv4IPMulticastMLDVersion OBJECT-TYPE
        SYNTAX           INTEGER { none(0), version1(1), version2(2) }
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) MLD multicast version."
        ::= { altasecHv4NetScalars 7 }
       
-- ========================
-- IPv4 Networking Table
-- =========================        
altasecHv4IPv4IfaceTable    OBJECT-TYPE
    SYNTAX                  SEQUENCE OF AltasecHv4IPv4IfaceEntry
    MAX-ACCESS              not-accessible
    STATUS                  current
    DESCRIPTION
        "(U//FOUO) The table containing a list of interface objects for
         the altasesec Haipe 4.0 IPv4 Interface table.
         This table contains information for both PT and CT interfaces.
         It augments the hIPv4IfaceTableEntry in the HAIPE-V4-NETWORKING-MIB."
    ::= { altasecHv4IPv4Augmentation 1 }

altasecHv4IPv4IfaceEntry      OBJECT-TYPE
    SYNTAX                  	AltasecHv4IPv4IfaceEntry
    MAX-ACCESS              	not-accessible
    STATUS                  	current
    DESCRIPTION
        "(U//FOUO) A row mapping IPv4 interface values to an interface."
    AUGMENTS { hIPv4IfaceTableEntry }
    ::= { altasecHv4IPv4IfaceTable 1 }

AltasecHv4IPv4IfaceEntry ::= SEQUENCE
  {
     altasecHv4IPv4IfaceICMPProcessPMTU            TruthValue,
     altasecHv4IPv4IfaceICMPProcessPMTUSetting     TruthValue,
     altasecHv4IPv4IfaceDHCP                       TruthValue,
     altasecHv4IPv4IfaceARPCacheTimeout            Unsigned32,
     altasecHv4IPv4IfaceProxyARP                   TruthValue,
     altasecHv4IPv4IfaceICMPProcessPING            TruthValue
  }
altasecHv4IPv4IfaceICMPProcessPMTU  OBJECT-TYPE
    SYNTAX                          TruthValue
    MAX-ACCESS                      read-only
    STATUS                          current
    DESCRIPTION
        "(U//FOUO,PR) Allows IPv4 Error ICMP messages (such as PMTU
         Discovery, and Source Quench) to be processed by the Linux kernel,
         these are then bypassed out to the PT network after being handled
         internally.

         (U//FOUO,PR) For the CT interface, if IPv4 Fixed Packet Length
         (FPL) is enabled, then this object is internally forced to False.
         
         (U//FOUO,PR) To read and write to its user setting see the
         altasecHv4IPv4IfaceICMPProcessPMTUSetting object. If FPL is 
         currently enabled, the user setting will be utilized once FPL 
         is disabled.
         
         (U//FOUO,PR) The PT interface is not affected by such behavior and  
         its value will mirror that of the altasecIfv4ICMPProcessPMTUSetting 
         object."
    ::= { altasecHv4IPv4IfaceEntry 1 }

altasecHv4IPv4IfaceICMPProcessPMTUSetting	OBJECT-TYPE
	SYNTAX                                  TruthValue
	MAX-ACCESS                              read-write
	STATUS                                  current
	DESCRIPTION
    	"(U//FOUO,PR) Allows IPv4 Error ICMP messages (such as PMTU
         Discovery, and Source Quench) to be processed by the Linux kernel,
         these are then bypassed out to the PT network after being handled
         internally. This is true(1) by default for the PT  interface and
         false(2) by default for the CT interface.

         (U//FOUO,PR) Note: This object is always writeable and contains,
         for the CT interface, the user setting to be applied should IPv4
         Fixed Packet Length (FPL) be disabled. To verify the current
         operational value for this object see the
         altasecIfv4ICMPProcessPMTU object."
	DEFVAL { false }
	::= { altasecHv4IPv4IfaceEntry 2 }

altasecHv4IPv4IfaceDHCP       OBJECT-TYPE
    SYNTAX                    TruthValue
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) Interface DHCP enable Truth Value."
    DEFVAL { false }
    ::= { altasecHv4IPv4IfaceEntry 3 }

altasecHv4IPv4IfaceARPCacheTimeout    OBJECT-TYPE
    SYNTAX                            Unsigned32
    MAX-ACCESS                        read-write
    STATUS                            current
    DESCRIPTION
        "(U//FOUO,PR) ARP cache timeout in seconds."
    DEFVAL { 300 }
    ::= { altasecHv4IPv4IfaceEntry 4 }

altasecHv4IPv4IfaceProxyARP       OBJECT-TYPE
    SYNTAX                        TruthValue
    MAX-ACCESS                    read-write
    STATUS                        current
    DESCRIPTION
        "(U//FOUO,PR) Proxy ARP allows the PT (Red) interface to respond to
         ARP requests for hosts connected by a security association.
         
         (U//FOUO,PR)  Note: This only applies to the PT interface. 
         Values set to the CT interface will return error."
    DEFVAL { false }
    ::= { altasecHv4IPv4IfaceEntry 5 }  

altasecHv4IPv4IfaceICMPProcessPING  OBJECT-TYPE
    SYNTAX                          TruthValue
    MAX-ACCESS                      read-write
    STATUS                          current
    DESCRIPTION
        "(U//FOUO,PR) Allows IPv4 PING messages to be processed by the PT 
         Interface."

    ::= { altasecHv4IPv4IfaceEntry 6 }

-- =========================
-- IPv6 Networking Table
-- =========================

altasecHv4IPv6IfaceTable     OBJECT-TYPE
    SYNTAX              	  SEQUENCE OF AltasecHv4IPv6IfaceEntry
    MAX-ACCESS          	  not-accessible
    STATUS              	  current
    DESCRIPTION
        "(U//FOUO,PR) The table containing a list of ViaSat-specific 
         interface objects for altasec Haipe 4.0 IPv6 interface table.
         It augments the hIPv6IfaceTableEntry in the 
         HAIPE-V4-NETWORKING-MIB."
    ::= { altasecHv4IPv6Augmentation 1 }

altasecHv4IPv6IfaceEntry         OBJECT-TYPE
    SYNTAX                       AltasecHv4IPv6IfaceEntry
    MAX-ACCESS                   not-accessible
    STATUS                       current
    DESCRIPTION
        "(U//FOUO,PR) A row mapping IPv6 interface values to an interface."
    AUGMENTS { hIPv6IfaceTableEntry }
    ::= { altasecHv4IPv6IfaceTable 1 }

AltasecHv4IPv6IfaceEntry ::= SEQUENCE
  {
     altasecHv4IPv6IfaceICMPProcessPMTU          TruthValue,
     altasecHv4IPv6IfaceICMPProcessPMTUSetting   TruthValue,
     altasecHv4IPv6IfaceICMPProcessPING          TruthValue
  }

altasecHv4IPv6IfaceICMPProcessPMTU    OBJECT-TYPE
    SYNTAX                            TruthValue
    MAX-ACCESS                        read-only
    STATUS                            current
    DESCRIPTION
        "(U//FOUO,PR) Allows IPv6 Error ICMP messages (such as PMTU
         Discovery or Destination Unreachable) to be processed by the Linux
         kernel.

         (U//FOUO,PR) For the CT interface, if IPv6 Fixed Packet Length
         (FPL) is enabled, then this object is internally forced to False.
         
         (U//FOUO,PR)  To read and write to its user setting see the
         altasecHv4IPv6IfaceICMPProcessPMTUSetting object. If FPL is 
         currently enabled, the user setting will be utilized once FPL 
         is disabled.
         
         (U//FOUO,PR) The PT interface is not affected by such behavior and 
         its value will mirror that of the 
         altasecHv4IPv6IfaceICMPProcessPMTUSetting object."
    ::= { altasecHv4IPv6IfaceEntry 1 }

altasecHv4IPv6IfaceICMPProcessPMTUSetting    OBJECT-TYPE
    SYNTAX                                   TruthValue
    MAX-ACCESS                               read-write
    STATUS                                   current
    DESCRIPTION
        "(U//FOUO,PR) Allows IPv6 Error ICMP messages (such as PMTU
         Discovery or Destination Unreachable) to be processed by the 
         Linux kernel. 
         
         (U//FOUO,PR)  This is true(1) by default for the PT interface and
         false(2) by default for the CT interface.

         (U//FOUO,PR) Note: This object is always writeable and contains,
         for the CT interface, the user setting to be applied should IPv6
         Fixed Packet Length (FPL) be disabled.
          
         (U//FOUO,PR)  To verify the current operational value for this 
         object see the  altasecHv4IPv6Iface6ICMPProcessPMTU object."
    ::= { altasecHv4IPv6IfaceEntry 2 }
    
altasecHv4IPv6IfaceICMPProcessPING  OBJECT-TYPE
    SYNTAX                          TruthValue
    MAX-ACCESS                      read-write
    STATUS                          current
    DESCRIPTION
        "(U//FOUO,PR) Allows IPv6 PING messages to be processed by the PT 
         Interface."

    ::= { altasecHv4IPv6IfaceEntry 3 }

