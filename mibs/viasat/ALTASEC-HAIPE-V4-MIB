altasecHv4DGSharedKeyTable  OBJECT-TYPE
    SYNTAX                    SEQUENCE OF AltasecHv4DGSharedKeyEntry
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) The table containing proprietary information about
        HAIPE Device Generated Shared Keys (DGSKs)."
    ::= { altasecHv4DGSharedKeyInformation 3 }

altasecHv4DGSharedKeyEntry  OBJECT-TYPE
    SYNTAX            AltasecHv4DGSharedKeyEntry
    MAX-ACCESS        not-accessible
    STATUS            current
    DESCRIPTION
        "(U//FOUO,PR) A row corresponding to a Device Generated Shared Key (DGSK)"
        AUGMENTS   { hDGSharedKeyTableEntry }
    ::= { altasecHv4DGSharedKeyTable 1 }

AltasecHv4DGSharedKeyEntry ::= SEQUENCE
    {
      altasecHv4DGSKEdition         PreplacedKeyEdition,
      altasecHv4DGSKSegment         PreplacedKeySegment,
      altasecHv4DGSKEndDate         DateAndTime
    }
    
altasecHv4DGSKEdition           OBJECT-TYPE
        SYNTAX          PreplacedKeyEdition
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The edition of this Device Generated Shared 
            key (DGSK)."
        ::= { altasecHv4DGSharedKeyEntry 2 }

altasecHv4DGSKSegment           OBJECT-TYPE
        SYNTAX          PreplacedKeySegment
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The segment number of this Device Generated Shared 
            key (DGSK)."
        ::= { altasecHv4DGSharedKeyEntry 3 }
        
altasecHv4DGSKEndDate   OBJECT-TYPE
        SYNTAX          DateAndTime
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The effective end date and time of this
            Device Generated Shared Key (DGSK).  This object will be 
            automatically set from the chain information once the spot in 
            the chain is reached where this key becomes actively used. 
            This column only exists if the key is assigned as a Shared Secret."
        ::= { altasecHv4DGSharedKeyEntry 4 }   

