-- *************************************************
-- IPMEIR DGSK Key Information.
--
altasecHv4IpmeirGenerateSharedKey OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-write
        STATUS      current
        DESCRIPTION
            "(U//FOUO) Setting this object to 'true' will force the generation 
             of a raw string (IPMEIR DGSK) to be used as key material. 
             Post-generation, the agent must create an entry in the 
             altasecHv4IpmeirDGSharedKeyTable that captures information on this
             DGSK.

             (U//FOUO) Note after being set to true, an agent should reset this 
             object to false once it has generated the DGSK and configured an 
             entry in the hDGSharedKeyTable." 
        ::= { altasecHv4DGSharedKeyInformation 4 }
        
altasecHv4IpmeirDGSharedKeyTableCount OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) The number of rows in the altasecHv4IpmeirDGSharedKeyTable."
        ::= { altasecHv4DGSharedKeyInformation 5 }
        
altasecHv4IpmeirDGSharedKeyTableLastChanged OBJECT-TYPE
        SYNTAX      TimeStamp
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "(U//FOUO,PR) The value of sysUpTime the last time any entry
            in the altasecHv4IpmeirDGSharedKeyTable was modified, created, or
            deleted by either SNMP or other management method
            (e.g. via an HMI).

            (U//FOUO,PR) Managers can use this object to ensure that no
            changes to configuration of this table have happened
            since the last time it examined the table.

            (U//FOUO,PR) A value of 0 indicates that no entry has been
            changed since the agent initialized."
        ::= { altasecHv4DGSharedKeyInformation 6 }

altasecHv4IpmeirDGSharedKeyTable  OBJECT-TYPE
    SYNTAX                    SEQUENCE OF AltasecHv4IpmeirDGSharedKeyEntry
    MAX-ACCESS                not-accessible
    STATUS                    current   
    DESCRIPTION
        "(U//FOUO,PR) The table containing proprietary information about
        IPMEIR Device Generated Shared Keys (DGSKs) and IPMEIR sequences."
    ::= { altasecHv4DGSharedKeyInformation 7 }
    
altasecHv4IpmeirDGSharedKeyEntry  OBJECT-TYPE
    SYNTAX            AltasecHv4IpmeirDGSharedKeyEntry
    MAX-ACCESS        not-accessible
    STATUS            current
    DESCRIPTION
        "(U//FOUO,PR) A row corresponding to an IPMEIR Device Generated Shared 
        Key (DGSK)"
    INDEX  { altasecHv4IpmeirDGSKIndex }
    ::= { altasecHv4IpmeirDGSharedKeyTable 1 }

AltasecHv4IpmeirDGSharedKeyEntry ::= SEQUENCE
    {
      altasecHv4IpmeirDGSKIndex           Unsigned32,
      altasecHv4IpmeirDGSKShortTitle      DGSharedKeyShortTitle,
      altasecHv4IpmeirDGSKEdition         PreplacedKeyEdition,
      altasecHv4IpmeirDGSKSegment         PreplacedKeySegment,
      altasecHv4IpmeirDGSKEffectiveDate   DateAndTime,
      altasecHv4IpmeirDGSKEndDate         DateAndTime,
      altasecHv4IpmeirDGSKLabel           SnmpAdminString,
      altasecHv4IpmeirDGSKMaterial        OCTET STRING,
      altasecHv4IpmeirDGSKType            INTEGER,
      altasecHv4IpmeirDGSKString          OCTET STRING,
      altasecHv4IpmeirDGSKIpv4Begin       IPAddress,
      altasecHv4IpmeirDGSKIpv4End         IPAddress,
      altasecHv4IpmeirDGSKIpv6Begin       IPAddress,
      altasecHv4IpmeirDGSKIpv6End         IPAddress,
      altasecHv4IpmeirDGSKIsLocal         TruthValue,
      altasecHv4IpmeirDGSKRowStatus       RowStatus
    }
    
altasecHv4IpmeirDGSKIndex OBJECT-TYPE
        SYNTAX     Unsigned32 
        MAX-ACCESS not-accessible
        STATUS     current
        DESCRIPTION
            "(U//FOUO) A unique numeric index that identifies this IPMEIR DGSK 
             or sequence"
        ::= { altasecHv4IpmeirDGSharedKeyEntry 1 }

altasecHv4IpmeirDGSKShortTitle OBJECT-TYPE
        SYNTAX     DGSharedKeyShortTitle 
        MAX-ACCESS read-only
        STATUS     current
        DESCRIPTION
            "(U//FOUO) The device-generated short title identifying this DGSK.
            
             (U//FOUO) If this is a sequence entry this column must not exist."
        ::= { altasecHv4IpmeirDGSharedKeyEntry 2 }
        
altasecHv4IpmeirDGSKEdition           OBJECT-TYPE
        SYNTAX          PreplacedKeyEdition
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The edition of this Device Generated Shared 
            key (DGSK). This column does not exist for sequence
            entries."
        ::= { altasecHv4IpmeirDGSharedKeyEntry 3 }

altasecHv4IpmeirDGSKSegment           OBJECT-TYPE
        SYNTAX          PreplacedKeySegment
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The segment number of this Device Generated Shared 
            key (DGSK). This column does not exist for sequence
            entries."
        ::= { altasecHv4IpmeirDGSharedKeyEntry 4 }

altasecHv4IpmeirDGSKEffectiveDate OBJECT-TYPE
        SYNTAX     DateAndTime
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) The effective date of this device-generated raw string. 
            This column does not exist for sequence entries."
        ::= { altasecHv4IpmeirDGSharedKeyEntry 5 }
        
altasecHv4IpmeirDGSKEndDate   OBJECT-TYPE
        SYNTAX          DateAndTime
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The effective end date and time of this
            Device Generated Shared Key (DGSK).  This object will be 
            automatically set from the chain information once the spot in 
            the chain is reached where this key becomes actively used. 
            This column only exists if the key is assigned as a Shared Secret."
        ::= { altasecHv4IpmeirDGSharedKeyEntry 6 }
        
altasecHv4IpmeirDGSKLabel OBJECT-TYPE
        SYNTAX     SnmpAdminString
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) A free text field allowing the user to tag the device-
             generated raw string with helpful information. For example, a label
             could indicate the intended usage of the device-generated raw
             string.
             
             (U//FOUO) If this entry relates to an IPMEIR sequence, the name
             of the sequence is returned. The name of a sequenceis not writable"
        ::= { altasecHv4IpmeirDGSharedKeyEntry 7 } 
        
altasecHv4IpmeirDGSKMaterial OBJECT-TYPE
        SYNTAX     OCTET STRING (SIZE(36))
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
             "(U//FOUO) The 72 hexadecimal character representation of the
             device-generated raw string (CRC included). Implementations must 
             restrict the display of this value to four hex characters at a 
             time. This column does not exist for sequence
             entries."
        ::= { altasecHv4IpmeirDGSharedKeyEntry 8 }

altasecHv4IpmeirDGSKType   OBJECT-TYPE
        SYNTAX     INTEGER { none(0), ipv4(1), ipv6(2), ipv4AndIpv6(3), 
                             domainName(4), email(5), keyId(6) }    
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) The IPMEIR Device Generated Shared Key type.
             
             Note that setting this column to any value other than 'none'
             causes the key to be assigned as a Shared Secret as long as
             the appropriate columns are also entered.
             
             If this DGSK has not been assigned as a shared secret this column
             should return none."
            
        ::= { altasecHv4IpmeirDGSharedKeyEntry 9 }
        
altasecHv4IpmeirDGSKString   OBJECT-TYPE
        SYNTAX     OCTET STRING    
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) If the DGSK is assigned with a type of domainName, 
             email, or keyId this column contains the string to set the value 
             to.  If this key is not one of those types this column should not
             exist."
            
        ::= { altasecHv4IpmeirDGSharedKeyEntry 10 }
        
altasecHv4IpmeirDGSKIpv4Begin   OBJECT-TYPE
        SYNTAX     IPAddress    
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) If the DGSK is assigned with a type of ipv4 or 
             ipv4AndIpv6 this column contains the begin IPv4 address to use. 
             If this key is not one of those types this column should not 
             exist."
            
        ::= { altasecHv4IpmeirDGSharedKeyEntry 11 }
        
altasecHv4IpmeirDGSKIpv4End   OBJECT-TYPE
        SYNTAX     IPAddress    
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) If the DGSK is assigned with a type of ipv4 or 
             ipv4AndIpv6 this column contains the end IPv4 address to use. 
             If this key is not one of those types this column should not 
             exist."
            
        ::= { altasecHv4IpmeirDGSharedKeyEntry 12 }

altasecHv4IpmeirDGSKIpv6Begin   OBJECT-TYPE
        SYNTAX     IPAddress    
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) If the DGSK is assigned with a type of ipv6 or 
             ipv4AndIpv6 this column contains the begin IPv6 address to use. 
             If this key is not one of those types this column should not 
             exist."
            
        ::= { altasecHv4IpmeirDGSharedKeyEntry 13 }
        
altasecHv4IpmeirDGSKIpv6End   OBJECT-TYPE
        SYNTAX     IPAddress    
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) If the DGSK is assigned with a type of ipv6 or 
             ipv4AndIpv6 this column contains the end IPv6 address to use. 
             If this key is not one of those types this column should not 
             exist."
            
        ::= { altasecHv4IpmeirDGSharedKeyEntry 14 }
        
altasecHv4IpmeirDGSKIsLocal   OBJECT-TYPE
        SYNTAX     TruthValue    
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) This column specifies whether this key will be set as 
             the local key."
            
        ::= { altasecHv4IpmeirDGSharedKeyEntry 15 }
           
altasecHv4IpmeirDGSKRowStatus OBJECT-TYPE
        SYNTAX     RowStatus  
        MAX-ACCESS read-create
        STATUS     current
        DESCRIPTION
            "(U//FOUO) The status of the row by which new entries may be created,
             existing entries enabled/disabled or existing entries deleted from
             this table.
        
             (U//FOUO) Entries created within this table may not become active
             unless all read-create columns of this table have valid values, as
             detailed by each individual column's description. Entries may be
             active, however, even though they are currently not used to protect
             traffic. For example, a device-generated raw string intended for 
             future use would have an effective start date beyond the current 
             time.
             
             (U//FOUO) Setting this column to destroy is synonymous
             with zeroizing the device-generated raw string. Any reference(s) 
             to this object upon setting this RowStatus to 'destroy' should be
             destroyed as well.
              
             (U//FOUO) At a minimum, implementations must support the 
             createAndGo, active and destroy management functions. Support for 
             createAndWait, notInService and notReady management functions is 
             optional.

             (U//FOUO) Destroying a sequence entry also destroys all keys associated
             with that sequence. " 
        ::= { altasecHv4IpmeirDGSharedKeyEntry 16 }

