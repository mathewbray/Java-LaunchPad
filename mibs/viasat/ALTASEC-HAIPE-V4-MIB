-- =========================
-- SFP table
-- =========================

altasecHv4SfpTable      OBJECT-TYPE
    SYNTAX              SEQUENCE OF AltasecHv4SfpEntry
    MAX-ACCESS          not-accessible
    STATUS              current
    DESCRIPTION
        "(U//FOUO,PR) The table containing a list of ViaSat-specific 
         SFP interface objects."
    ::= { altasecHv4SFP 1 }

altasecHv4SfpEntry      OBJECT-TYPE
    SYNTAX              AltasecHv4SfpEntry
    MAX-ACCESS          not-accessible
    STATUS              current
    DESCRIPTION
        "(U//FOUO) A row mapping interface variables for the PT and CT 
        SFP interfaces."
    INDEX   { altasecHv4SfpSide, altasecHv4IfIndex }
    ::= { altasecHv4SfpTable 1 }

AltasecHv4SfpEntry ::= SEQUENCE
    {
        altasecHv4SfpSide        Unsigned32,
        altasecHv4SfpIndex       Unsigned32,
        altasecHv4SfpStatus      INTEGER,
        altasecHv4SfpTrancver    OCTET STRING,
        altasecHv4SfpVendorOui   INTEGER,
        altasecHv4SfpVendorRev   OCTET STRING,
        altasecHv4SfpVendorName  OCTET STRING,
        altasecHv4SfpVendorPn    OCTET STRING
    }

altasecHv4SfpSide       OBJECT-TYPE
     SYNTAX             Unsigned32
     MAX-ACCESS         read-only
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) An numeric index that identifies the side of the 
	 interface."
     ::= { altasecHv4SfpEntry 1 }
 
altasecHv4SfpIndex      OBJECT-TYPE
     SYNTAX             Unsigned32
     MAX-ACCESS         read-only
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) An unique numeric index that identifies interface."
     ::= { altasecHv4SfpEntry 2 }    

altasecHv4SfpStatus	OBJECT-TYPE
     SYNTAX             INTEGER { sfpActionAllow(1),
                                  sfpActionWarn(2),
                                  sfpActionDisable(4)
                                }
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) A numeric value indicate the state of the SFP link."
     ::= { altasecHv4SfpEntry 3 }    

altasecHv4SfpTrancver	OBJECT-TYPE
     SYNTAX             OCTET STRING
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) Value that holds detailed information about the SFP."
     ::= { altasecHv4SfpEntry 4 }    

altasecHv4SfpVendorOui	OBJECT-TYPE
     SYNTAX             INTEGER
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) The SFP vendor OUI."
     ::= { altasecHv4SfpEntry 5 }    

altasecHv4SfpVendorRev	OBJECT-TYPE
     SYNTAX             OCTET STRING
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) The SFP vendor revision number."
     ::= { altasecHv4SfpEntry 6 }    

altasecHv4SfpVendorName	OBJECT-TYPE
     SYNTAX             OCTET STRING
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) The SFP vendor name."
     ::= { altasecHv4SfpEntry 7 }    

altasecHv4SfpVendorPn	OBJECT-TYPE
     SYNTAX             OCTET STRING
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) The SFP vendor part number."
     ::= { altasecHv4SfpEntry 8 }    
       
