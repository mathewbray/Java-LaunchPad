-- =========================
-- Address Assignment table
-- =========================

altasecHv4AddrAssignTable OBJECT-TYPE
    SYNTAX           SEQUENCE OF AltasecHv4AddrAssignEntry
    MAX-ACCESS       not-accessible
    STATUS           current
    DESCRIPTION
        "(U//FOUO) The table containing a list of ViaSat-specific 
         objects for the altasec Haipe 4.0 Address Assignment table.
         This table augments the hAddressAssignTable in the 
         HAIPE-V4-NETWORKING-MIB."       
    ::= { altasecHv4AddrAssign 1 }

altasecHv4AddrAssignEntry OBJECT-TYPE
    SYNTAX           AltasecHv4AddrAssignEntry
    MAX-ACCESS       not-accessible
    STATUS           current
    DESCRIPTION
        "(U//FOUO) A row which describes an interface to IP address mapping
        This table is indexed by interface id, ip type, and ip address
        corrsponding with the hAddressAssignTable"
    AUGMENTS  { hAddressAssignTableEntry }
    ::= { altasecHv4AddrAssignTable 1 }

AltasecHv4AddrAssignEntry ::= SEQUENCE
{
   altasecHv4AddrAssignAddress            IPAddress,
   altasecHv4AddrAssignPrefixAdverOnly    TruthValue    
}

altasecHv4AddrAssignAddress OBJECT-TYPE
    SYNTAX                 IPAddress
    MAX-ACCESS             read-write
    STATUS                 current
    DESCRIPTION
        "(U//FOUO) The Internet Protocol addressing information for the
         interface indicated by this row. The size and format of this column
         is determined by the hAddressAssignType in the hAddressAssignTable.
         
         This object is used to modify the address of an existing IP address 
         currently configured(assigned) to an interface.  Note that during a 
         write (modify) this value will be different than the value of the 
         Address specfied as the INDEX for this table, i.e hAddressAssignAddr.  
         During a read this value will be the same as hAddressAssignAddr. "
    ::= { altasecHv4AddrAssignEntry 1 }

altasecHv4AddrAssignPrefixAdverOnly OBJECT-TYPE
    SYNTAX                 TruthValue
    MAX-ACCESS             read-only
    STATUS                 current
    DESCRIPTION
        "(U//FOUO) This object indicates whether the entry identified by
         the interface, type, and address instance values is designated 
         only to provide Prefix/Router Advertisement."
    ::= { altasecHv4AddrAssignEntry 2 }
       

-- =========================
-- Interface table
-- =========================

altasecHv4IfTable       OBJECT-TYPE
    SYNTAX              SEQUENCE OF AltasecHv4IfEntry
    MAX-ACCESS          not-accessible
    STATUS              current
    DESCRIPTION
        "(U//FOUO,PR) The table containing a list of ViaSat-specific 
         interface objects."
    ::= { altasecHv4Interface 1 }

altasecHv4IfEntry       OBJECT-TYPE
    SYNTAX              AltasecHv4IfEntry
    MAX-ACCESS          not-accessible
    STATUS              current
    DESCRIPTION
        "(U//FOUO) A row mapping interface variables for the PT and CT 
        interfaces."
    INDEX   { altasecHv4IfIndex }
    ::= { altasecHv4IfTable 1 }

AltasecHv4IfEntry ::= SEQUENCE
    {
        altasecHv4IfIndex                   Unsigned32,
        altasecHv4IfDuplex                  INTEGER,
        altasecHv4IfMode                    INTEGER,
        altasecHv4IfNegotiateSpeedDuplex    BITS,
        altasecHv4IfForceSpeed              Unsigned32,
        altasecHv4IfForceDuplex             INTEGER
    }

altasecHv4IfIndex       OBJECT-TYPE
     SYNTAX             Unsigned32
     MAX-ACCESS         not-accessible
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) An unique numeric index that identifies this interface.
         Interface Index column is the primary index column for this table.
         Note, this column is populated internally and can not be read or
         configured."
     ::= { altasecHv4IfEntry 1 }
     
altasecHv4IfDuplex      OBJECT-TYPE
     SYNTAX             INTEGER { unknown(0),
                                  halfDuplex(1),
                                  fullDuplex(2)
                                }
     MAX-ACCESS         read-only
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) Operational duplex setting of the current link."
     ::= { altasecHv4IfEntry 2 }

altasecHv4IfMode        OBJECT-TYPE
     SYNTAX             INTEGER { autoNegotiate(1), force(2) }
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) The mode by which link speed and duplex should be
          determined."
     DEFVAL { autoNegotiate }
     ::= { altasecHv4IfEntry 3 }

altasecHv4IfNegotiateSpeedDuplex OBJECT-TYPE
     SYNTAX             BITS { speed10MbitHalfDuplex(0),
                               speed10MbitFullDuplex(1),
                               speed100MbitHalfDuplex(2),
                               speed100MbitFullDuplex(3)
                             }
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) The speed and duplex modes that should be offered for
          negotiation. These settings are utilized when the altasecHv4IfMode
          object is set to autoNegotiate(1)."
     DEFVAL { { speed10MbitHalfDuplex, speed10MbitFullDuplex,
                speed100MbitHalfDuplex, speed100MbitFullDuplex } }
     ::= { altasecHv4IfEntry 4 }

altasecHv4IfForceSpeed  OBJECT-TYPE
     SYNTAX             Unsigned32
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) The speed mode that should be forced. This setting
          is utilized when the altasecHv4IfMode object is set to force(2)."
     DEFVAL { 100000000 }
     ::= { altasecHv4IfEntry 5 }

altasecHv4IfForceDuplex OBJECT-TYPE
     SYNTAX             INTEGER { unknown(0),
                                  halfDuplex(1),
                                  fullDuplex(2)
                                }
     MAX-ACCESS         read-write
     STATUS             current
     DESCRIPTION
         "(U//FOUO, PR) The duplex mode that should be forced. This setting
          is utilized when the altasecHv4IfMode object is set to force(2)."
     DEFVAL { fullDuplex }
     ::= { altasecHv4IfEntry 6 }
        

