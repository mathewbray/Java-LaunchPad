altasecHv4ManSAKeyChainTableCount OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) The number of rows in the 
            altasecHv4ManSAKeyChainTable."
        ::= { altasecHv4ChainInformation 1 }

altasecHv4ManSAKeyChainTableLastChanged OBJECT-TYPE
        SYNTAX      	TimeStamp
        MAX-ACCESS  	read-only
        STATUS      	current
        DESCRIPTION
            "(U//FOUO,PR) The value of sysUpTime the last time any entry
            in the altasecHv4ManSAKeyChainTable was modified, created, or
            deleted by either SNMP or other management method
            (e.g. via an HMI).

            (U//FOUO,PR) Managers can use this object to ensure that no
            changes to configuration of this table have happened
            since the last time it examined the table.

            (U//FOUO,PR) A value of 0 indicates that no entry has been
            changed since the agent initialized."
        ::= { altasecHv4ChainInformation 2 }

altasecHv4ManSAKeyChainTable  OBJECT-TYPE
    SYNTAX                    SEQUENCE OF AltasecHv4ManSAKeyChainEntry
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) The table containing proprietary information about
        pre-placed key (PPK) chains.  This table includes a column to
        set the integrity algorithm at creation time for the PPK Chain."
    ::= { altasecHv4ChainInformation 3 }

altasecHv4ManSAKeyChainEntry  OBJECT-TYPE
    SYNTAX            AltasecHv4ManSAKeyChainEntry
    MAX-ACCESS        not-accessible
    STATUS            current
    DESCRIPTION
        "(U//FOUO,PR) A row indicating a pre-placed key (PPK) within a
        chain of PPK's and the order that the PPK indicated will be
        used by the HAIPE device as part of that chain."
        AUGMENTS   { hManSAKeyChainTableEntry }
    ::= { altasecHv4ManSAKeyChainTable 1 }

AltasecHv4ManSAKeyChainEntry ::= SEQUENCE
    {
        altasecManSAKeyChainTransformSuite  		AltasecIntegrityAlgorithm,
        altasecManSAKeyChainEncryptionAlgorithm		AltasecEncryptionAlgorithm,
    	altasecManSAKeyChainPPKSPID					INTEGER					
    }

altasecManSAKeyChainTransformSuite  OBJECT-TYPE
    SYNTAX                          AltasecIntegrityAlgorithm
    MAX-ACCESS                      read-write
    STATUS                          current
    DESCRIPTION
        "(U//FOUO,PR) The integrity algorithm to be used by this key.
         This integrity algorithm should be manually configured for
         the first key assigned to the chain but later keys added to
         the same chain will have this integrity algorithm field set
         to the same integrity algorithm from the previous key in the
         chain."
    ::= { altasecHv4ManSAKeyChainEntry 1 }
    
altasecManSAKeyChainEncryptionAlgorithm  OBJECT-TYPE
    SYNTAX                          	 AltasecEncryptionAlgorithm
    MAX-ACCESS                      	 read-write
    STATUS                          	 current
    DESCRIPTION
        "(U//FOUO) The encryption algorithm to be used by this key.
		This encryption algorithm should be manually configured for
		the first key assigned to the chain but later keys added to
		the same chain will have this encryption algorithm field set
		to the same encryption algorithm from the previous key in the
		chain.  When a given key becomes active and is used to protect
		traffic, the hPPKTrafficEncryptAlgorithm column for this
		key in the chain will update to match the value in this
		column."
    ::= { altasecHv4ManSAKeyChainEntry 2 }
    
altasecManSAKeyChainPPKSPID				 OBJECT-TYPE
    SYNTAX                          	 INTEGER
    MAX-ACCESS                      	 read-only
    STATUS                          	 current
    DESCRIPTION
        "(U//FOUO) The SPID of the PrePlacedKey on this chain. "
    ::= { altasecHv4ManSAKeyChainEntry 3 }
    
-- *************************************************
-- DGSK Key Information.
