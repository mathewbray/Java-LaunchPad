    	altasecHv4LegacyDeliveryPriority        Unsigned32,
      altasecHv4LegacyDeliveryRangeStart      IPAddress,
      altasecHv4LegacyDeliveryRangeEnd        IPAddress,
      altasecHv4LegacyDeliveryStatus          OCTET STRING
    }
    
altasecHv4LegacyDeliveryPriority         OBJECT-TYPE
    SYNTAX                    Unsigned32
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO) A unique numeric index that indicates the priority of the GDC
         server in context. Lower values have a higher priority."
        
    ::= { altasecHv4LegacyDeliveryEntry 2 }
    
altasecHv4LegacyDeliveryRangeStart         OBJECT-TYPE
    SYNTAX                    IPAddress
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO) The starting IPv4 or IPv6 address for the range that
        will cause the device to initiate Dynamic Discovery."
        
    ::= { altasecHv4LegacyDeliveryEntry 3 }
    
