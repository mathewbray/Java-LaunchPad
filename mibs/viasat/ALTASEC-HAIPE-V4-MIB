-- **********************************************************************
-- Textual conventions
-- **********************************************************************

AltasecEncryptionAlgorithm ::= TEXTUAL-CONVENTION
        STATUS  current
        DESCRIPTION
            "(U//FOUO) An encryption algorithm."
        SYNTAX   INTEGER { unassigned(0),
                           baton(1), 
                           medley(2),
                           aes(3)
                         }

AltasecIntegrityAlgorithm ::= TEXTUAL-CONVENTION
        STATUS  current
        DESCRIPTION
            "(U//FOUO) A data integrity algorithm.  It is possible
            that in some situations it will not be possible to
            determine the correct algorithm to return since more than
            one are in use for a given key (for example).  For these
            cases the multiple(7) value should be returned."
        SYNTAX   INTEGER { unassigned(0),
                           gcm0(1), 
                           gcm32(2),
                           gcm64(3),
                           gcm96(4),
                           gcm128(5),
                           bip32(6),
                           multiple(7)
                         }
                         
AltasecHv4DiscoveryStatus ::= TEXTUAL-CONVENTION
        STATUS  current
        DESCRIPTION
            "(U//FOUO) The current status of a Discovery entry."
        SYNTAX   INTEGER { active(1),
                           inactive(2)
                         }
                         
-- *************************************************
-- Peer Discovery.

altasecHv4DiscoveryEnabled OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "(U//FOUO) The indication of whether the HAIPE will
        dynamically discover remote HAIPEs and establish an SA using
        the services listed in the hDiscoveryDeliveryTable."
    DEFVAL { true }
    ::= { altasecHv4PeerDiscovery 2 }
                      
