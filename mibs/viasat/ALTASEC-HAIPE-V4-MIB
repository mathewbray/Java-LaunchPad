-- =======================
-- HD3 Server Table
-- =======================

altasecHv4HD3SrvInfoTable OBJECT-TYPE
        SYNTAX         SEQUENCE OF AltasecHv4HD3SrvInfoEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION
            "(U//FOUO,PR) The table containing HD3 server information"
        ::= { altasecHv4HD3ServerInfo 1 }

altasecHv4HD3SrvInfoEntry OBJECT-TYPE
        SYNTAX         AltasecHv4HD3SrvInfoEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION
            "(U//FOUO,PR) The table entry containing HD3 server information"
        INDEX { altasecHv4HD3SrvInfoNum }
        ::= { altasecHv4HD3SrvInfoTable 1 }

AltasecHv4HD3SrvInfoEntry ::= SEQUENCE
    {
       altasecHv4HD3SrvInfoNum    Unsigned32,
       altasecHv4HD3SrvInfoNode   Unsigned32,
       altasecHv4HD3SrvInfoCTIP   IPAddress,
       altasecHv4HD3SrvInfoPTIP   IPAddress,
       altasecHv4HD3SrvInfoState  SnmpAdminString,
       altasecHv4HD3SrvInfoRole   SnmpAdminString,
       altasecHv4HD3SrvInfoMethod SnmpAdminString,
       altasecHv4HD3SrvRowStatus  RowStatus
    }

altasecHv4HD3SrvInfoNum       OBJECT-TYPE
        SYNTAX             Unsigned32
        MAX-ACCESS         read-only
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The HD3 Server Number associated with this row."
        ::= { altasecHv4HD3SrvInfoEntry 1 }

altasecHv4HD3SrvInfoNode      OBJECT-TYPE
        SYNTAX             Unsigned32
        MAX-ACCESS         read-only
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The HD3 Server Node ID associated with this row."
        ::= { altasecHv4HD3SrvInfoEntry 2 }

altasecHv4HD3SrvInfoCTIP      OBJECT-TYPE
        SYNTAX             IPAddress
        MAX-ACCESS         read-create
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The HD3 Server CT IP associated with this row."
        ::= { altasecHv4HD3SrvInfoEntry 3 }

altasecHv4HD3SrvInfoPTIP      OBJECT-TYPE
        SYNTAX             IPAddress
        MAX-ACCESS         read-create
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The HD3 Server PT IP associated with this row."
        ::= { altasecHv4HD3SrvInfoEntry 4 }

altasecHv4HD3SrvInfoState     OBJECT-TYPE
        SYNTAX             SnmpAdminString
        MAX-ACCESS         read-only
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The HD3 Server State associated with this row."
        ::= { altasecHv4HD3SrvInfoEntry 5 }

altasecHv4HD3SrvInfoRole      OBJECT-TYPE
        SYNTAX             SnmpAdminString
        MAX-ACCESS         read-only
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The HD3 Server Role associated with this row."
