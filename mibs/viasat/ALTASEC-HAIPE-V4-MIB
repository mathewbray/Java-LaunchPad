        SYNTAX          TruthValue
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Audit Log Compression enabled flag."
        ::= { altasecHv4MgmtAudit 3 }

altasecHv4AuditLogAdminStatus OBJECT-TYPE
        SYNTAX          INTEGER
                        {
                           allowAction(0),
                           retrieve(1),
                           delete(2)
                        }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) This scalar controls the audit log handling process.
             When read, it will return allowAction if the last action succeeded,
             or the last requested action, if an action failed or is still in
             progress."
        ::= { altasecHv4MgmtAudit 4 }

altasecHv4AuditLogOperStatus  OBJECT-TYPE
        SYNTAX          INTEGER
                        {
                           noPendingOperation(0),
                           retrievalInProgress(1),
                           retrievalComplete(2),
                           deletionInProgress(3),
                           deletionComplete(4),
                           retrievalFailed(5),
                           deletionFailed(6)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) This scalar indicates the current state of the audit log.
             retrievalInProgress(1) indicates that an audit log retrieval is
             underway. retrievalComplete(2) indicates that the most recent audit
             log has been successfully retrieved. deletionInProgress(3)
             indicates that an audit log deletion is underway.
             deletionComplete(4) indicates that the audit log has been deleted
             successfully. retrievalFailed(5) indicates that the retrieval of
             the audit log failed. deletionFailed(6) indicates that the deletion
             of the audit log failed."
        ::= { altasecHv4MgmtAudit 5 }

altasecHv4AuditLogWarningPercentage OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The percentage of capacity at which the
            altasecHv4AuditLogWarning notification will be generated. A
            value of zero indicates that the notification should never
            be sent.  HAIPEs with a fixed warning percentage will not
            support SET operations to this object."
        ::= { altasecHv4MgmtAudit 6 }
        
        
altasecHv4AuditLogCapacityPercentage OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The current utilized percentage
            of storage capacity for audit events. When the log reaches
            its full capacity, the hNotificationLogFull notification will
            be generated. The fate of new events will be determined by
            the hNotificationLogFullBehavior object."
        ::= { altasecHv4MgmtAudit 7 }
        
altasecHv4AuditLogFullBehavior OBJECT-TYPE
        SYNTAX          INTEGER
        				{
                           logWrap(1),
                           logDrop(2)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The object used for determining the fate of new
            audit events when the audit log has reached its maximum
            capacity. If the value is logWrap, new events will overwrite
            the oldest events. If the value is logDrop, new events will
            be dropped. Notifications for the event, if configured will
            still be sent.
			
            (U//FOUO) HAIPEs with a fixed behavior will not support SET
            operations to this object."
			
        ::= { altasecHv4MgmtAudit 8 }

altasecHv4AuditLogReset OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The object used for clearing the audit logs.
            Setting this object to true will reset (clear) all audit
            logs. When read, this object will always return false.
			
            (U//FOUO) Note that this will also reset the
            hAudCfgLogCount and hAuditCfgLogThrottled Unsigned32
            counters in the hAuditConfigTable."
        ::= { altasecHv4MgmtAudit 9 }
        
altasecHv4AuditLogTable		  OBJECT-TYPE
    SYNTAX                    SEQUENCE OF AltasecHv4AuditLogEntry
    MAX-ACCESS                not-accessible
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) The table containing proprietary information about
        alert log entries."
    ::= { altasecHv4MgmtAudit 10 }

