-- ********************************************************************
-- altasecIPhFFSATable
-- Table of FFSA Management
--

altasecIPhFFSATable        OBJECT-TYPE
        SYNTAX             SEQUENCE OF ALTASECIPhFFSAEntry
        MAX-ACCESS         not-accessible
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The table for configuring AltaSec proprietary
             extensions to the HAIPE-TRAFFIC-PROTECTION-MIB's hIPhFFSATable."
        ::= { altasecTPiphFFSAKey 3 }

altasecIPhFFSAEntry     OBJECT-TYPE
        SYNTAX          ALTASECIPhFFSAEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) A row which describes the proprietary information to
             further configure settings to setup a Security Association with a
             remote HAIPE."
        AUGMENTS   { hIPhFFSAEntry }
        ::= { altasecIPhFFSATable 1 }

ALTASECIPhFFSAEntry ::= SEQUENCE
    {
        altasecIPhFFSADSCPCopyValue                            OCTET STRING,
        altasecIPhFFSAECNValue                                 OCTET STRING,
        altasecIPhFFSAECNCopyValue                             OCTET STRING,
        altasecIPhFFSADFBitSetting                             INTEGER,
        altasecIPhFFSATunnelPreferredFragmentationSide         HaipeSide,
        altasecIPhFFSATunnelPreferredFragmentationSideSetting  HaipeSide,
        altasecIPhFFSAMulticastBypassMode                      TruthValue,
        altasecIPhFFSAMulticastResetSource                     TruthValue
    }

altasecIPhFFSADSCPCopyValue  OBJECT-TYPE
    SYNTAX                   OCTET STRING (SIZE (1))
    MAX-ACCESS               read-write
    STATUS                   current
    DESCRIPTION
        "(U//FOUO,PR) The Differentiated Service Code Points (DSCP)
         value for this SA.

         (U//FOUO,PR) The use of this value is dependent on the value
         of the hIPhFFSADSCP column in its parent hIPhFFSATable row being
         set to set(1). Any other values (acceptlist(3)) will result in
         sets to the object returning an inconsistentValue error.

         (U//FOUO,PR) When the hIPhFFSADSCP column is set to set(1), it
         implicitly sets this object to 0x00.

         (U//FOUO,PR) If the hIPhFFSADSCP column is set to set(1), this
         value should be one octet in length in the range 0x00 -
         0x3f. Any attempt to set this column to value outside of
         this range should, in this case, fail with an invalid value
         error. Any bits set in these lower/right-most 6 of the
         octet will indicate that those bits be copied from the
         incoming packet's Differentiated Service Code Points (DSCP)
         header value to the outgoing packet. Any bits set for this
         value effectively overrides, to copy, those set in the
         parent hIPhFFSADSCPValue column."
    DEFVAL { '00'h }
    ::= { altasecIPhFFSAEntry 3 }

altasecIPhFFSAECNValue  OBJECT-TYPE
    SYNTAX              OCTET STRING (SIZE (1))
    MAX-ACCESS          read-write
    STATUS              current
    DESCRIPTION
        "(U//FOUO,PR) The indication of how the Explicit Congestion
         Notification (ECN) bits should be treated within this SA.

         (U//FOUO,PR) The use of this value overrides the setting of the
         hIPhFFSAECN column in its parent hTPSATable, which will then return
         a no such instance error.

         (U//FOUO,PR) This value should be one octet in length: 0x00, 0x01,
         0x02, 0x03. Any attempt to set this column to value outside of this
         range should fail with a wrong value error.  The ECN value garnered
         from these lower/right-most 2 of the octet are placed within the
         outgoing packet's Explicit Congestion Notification (ECN) header value."
    DEFVAL { '00'h }
    ::= { altasecIPhFFSAEntry 4 }

altasecIPhFFSAECNCopyValue  OBJECT-TYPE
    SYNTAX                  OCTET STRING (SIZE (1))
    MAX-ACCESS              read-write
    STATUS                  current
    DESCRIPTION
        "(U//FOUO,PR) The indication of how the Explicit Congestion
         Notification (ECN) bits should be treated within this SA.

         (U//FOUO,PR) The use of this value overrides the setting of the
         hIPhFFSAECN column in its parent hTPSATable, which will then return
         a no such instance error.

         (U//FOUO,PR) This value should be one octet in length: 0x00, 0x01,
         0x02, 0x03. Any attempt to set this column to value outside of this
         range should fail with a wrong value error.  Any bits set in these
         lower/right-most 2 of the octet will indicate that those bits be
         copied from the incoming packet's Explicit Congestion Notification
         (ECN) header value to the outgoing packet. Any bits set for this
         value effectively overrides, to copy, those set in the above
         altasecIPhFFSAECNValue column."
    DEFVAL { '00'h }
    ::= { altasecIPhFFSAEntry 5 }

altasecIPhFFSADFBitSetting  OBJECT-TYPE
    SYNTAX                  INTEGER { set(1), clear(2), copy(3) }
    MAX-ACCESS              read-write
    STATUS                  current
    DESCRIPTION
        "(U//FOUO,PR) The user setting of how to handle the DF bit created in
         the CT packet: set it; clear it, or copy the DF bit from the PT packet.

         (U//FOUO,PR) Note: This column is only pertinent to IPv4 traffic that
         is being encrypted (i.e. PT->CT traffic).

         (U//FOUO,PR) Note: This object is always writeable and contains the
         user setting to be applied should IPv4 Fixed Packet Length (FPL) be
         disabled and PT be the Preferred Fragmentation Side as represented by
         the altasecIfv4TunnelPreferredFragmentationSide object. If IPv4 FPL is
         enabled, only 'Clear' is a valid operational value. If IPv4 FPL is
         disabled and CT is the Preferred Fragmentation Side, only 'Copy' and
         'Clear' may become operational. A user setting of 'Set' will translate
         into an operational setting of 'Clear'. Otherwise (IPv4 FPL off & PT
         Frag Side), all three settings are operationally valid. To verify the
         current operational value for this object, see hIPhFFSADFBit."
    DEFVAL { clear }
    ::= { altasecIPhFFSAEntry 6 }

altasecIPhFFSATunnelPreferredFragmentationSide  OBJECT-TYPE
        SYNTAX                               HaipeSide
        MAX-ACCESS                           read-only
        STATUS                               current
        DESCRIPTION
            "(U//FOUO,PR) The Operational Fragmentation Side (pt(1) or ct(2))
             for SAs. If the preferred fragmentation side is CT, then CT
             DFBit(s) will be ignored.

             (U//FOUO,PR) Note: This extension is only applicable to Tunnel SAs
             and will be ignored otherwise. If IPv4 Fixed Packet Length (FPL)
             is enabled, then this object is internally forced to PT. To read
             and write to its user setting (to be utilized once IPv4 FPL is
             disabled) see the
             altasecIPhFFSATunnelPreferredFragmentationSideSetting object."
        DEFVAL { pt }
        ::= { altasecIPhFFSAEntry 7 }

altasecIPhFFSATunnelPreferredFragmentationSideSetting  OBJECT-TYPE
        SYNTAX                                      HaipeSide
        MAX-ACCESS                                  read-write
        STATUS                                      current
        DESCRIPTION
            "(U//FOUO,PR) The desired Fragmentation Side Setting (pt(1) or
             ct(2)) for SAs.

             (U//FOUO,PR) Note: This object is always writeable and contains the
             user setting to be applied should FPL be disabled. To verify
             the current operational value for this object, see
             altasecIPhFFSATunnelPreferredFragmentationSide."
        DEFVAL { pt }
        ::= { altasecIPhFFSAEntry 8 }

altasecIPhFFSAMulticastBypassMode    OBJECT-TYPE
        SYNTAX                       TruthValue
        MAX-ACCESS                   read-write
        STATUS                       current
        DESCRIPTION
            "(U//FOUO,PR) The Multicast Bypass/Host support method for SAs."
        DEFVAL { false }
        ::= { altasecIPhFFSAEntry 9 }

altasecIPhFFSAMulticastResetSource   OBJECT-TYPE
        SYNTAX                       TruthValue
        MAX-ACCESS                   read-write
        STATUS                       current
        DESCRIPTION
            "(U//FOUO,PR) Causes decrypted multicast traffic to have the HAIPE's
             PT IP address as its source address, instead of the multicast
             sender's original IP address."
        DEFVAL { false }
        ::= { altasecIPhFFSAEntry 10 }

-- **********************************************************************
-- Security Association Database Objects   SAD
--

altasecTPSAManagement   OBJECT IDENTIFIER ::= { altasecTPSecurityAssocDatabase 1 }

altasecTPSAManagementSettings OBJECT IDENTIFIER ::= { altasecTPSAManagement 7 }

--
-- Table of SA Management
--

altasecTPSATable           OBJECT-TYPE
        SYNTAX             SEQUENCE OF ALTASECTPSAEntry
        MAX-ACCESS         not-accessible
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) This table supplements the information provided in the
             corresponding hTPSATable in the HAIPE-TRAFFIC-PROTECTION-MIB
             with additional proprietary values. The hTPSAIndex column is
             the only index for this table and references its parent
             entry in the aforementioned hTPSATable.

             (U//FOUO,PR) As part of the proprietary extension, configuration
             items that affect the behavior of the SAs may be modified, even
             if the SA is currently active."
        ::= { altasecTPSAManagement 3 }

altasecTPSAEntry        OBJECT-TYPE
        SYNTAX          ALTASECTPSAEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) A row describing the additional proprietary
             information about current Security Associations (SA's) that
             are not included in the HAIPE hTPSATable."
        AUGMENTS   { hTPSAEntry }
        ::= { altasecTPSATable 1 }

ALTASECTPSAEntry ::= SEQUENCE
    {
        altasecTPSADSCPCopyValue                      OCTET STRING,
        altasecTPSAECNValue                           OCTET STRING,
        altasecTPSAECNCopyValue                       OCTET STRING,
        altasecTPSATTLHopLimit                        INTEGER,
        altasecTPSADFBitSetting                       INTEGER,
        altasecTPSAName                               SnmpAdminString,
        altasecTPSAIsMulticast                        TruthValue,
        altasecTPSAMulticastBypassMode                TruthValue,
        altasecTPSAMulticastResetSource               TruthValue,
        altasecTPSAConnectionStatus                   INTEGER,
        altasecTPSATransformID                        INTEGER,
        altasecTPSATransportPacketsDropped            Counter64,
        altasecTPSATransportFragmentedPacketsDropped  Counter64
    }

altasecTPSADSCPCopyValue  OBJECT-TYPE
    SYNTAX                OCTET STRING (SIZE (1))
    MAX-ACCESS            read-write
    STATUS                current
    DESCRIPTION
        "(U//FOUO,PR) The Differentiated Service Code Points (DSCP)
         value for this SA.

         (U//FOUO,PR) The use of this value is dependent on the value
         of the hTPSADSCP column in its parent hTPSATable row being
         set to set(1). Any other values (acceptlist(3)) will result in
         this value being ignored.

         (U//FOUO,PR) If the hTPSADSCP column is set to set(1), this
         value should be one octet in length in the range 0x00 -
         0x3f. Any attempt to set this column to value outside of
         this range should, in this case, fail with an invalid value
         error. Any bits set in these lower/right-most 6 of the
         octet will indicate that those bits be copied from the
         incoming packet's Differentiated Service Code Points (DSCP)
         header value to the outgoing packet. Any bits set for this
         value effectively overrides, to copy, those set in the
         parent hTPSADSCPValue column."
    DEFVAL { '00'h }
    ::= { altasecTPSAEntry 2 }

altasecTPSAECNValue  OBJECT-TYPE
    SYNTAX           OCTET STRING (SIZE (1))
    MAX-ACCESS       read-write
    STATUS           current
    DESCRIPTION
        "(U//FOUO,PR) The indication of how the Explicit Congestion
         Notification (ECN) bits should be treated within this SA.

         (U//FOUO,PR) The use of this value is dependent on the value
         of the hTPSAECN column in its parent hTPSATable row being
         set to disabled(1). Any other values (congestionNotification(2),
         admissionControl(3)) will result in this value being ignored.

         (U//FOUO,PR) If the hTPSAECN column is set to disabled(1), this
         value should be one octet in length: 0x00, 0x01, 0x02, 0x03.
         Any attempt to set this column to value outside of this range
         should, in this case, fail with an wrong value error.  The
         ECN value garnered from these lower/right-most 2 of the octet
         are placed within the outgoing packet's Explicit Congestion
         Notification (ECN) header value."
    DEFVAL { '00'h }
    ::= { altasecTPSAEntry 3 }

altasecTPSAECNCopyValue  OBJECT-TYPE
    SYNTAX               OCTET STRING (SIZE (1))
    MAX-ACCESS           read-write
    STATUS               current
    DESCRIPTION
        "(U//FOUO,PR) The indication of how the Explicit Congestion
         Notification (ECN) bits should be treated within this SA.

         (U//FOUO,PR) The use of this value is dependent on the value
         of the hTPSAECN column in its parent hTPSATable row being
         set to disabled(1). Any other values (congestionNotification(2),
         admissionControl(3)) will result in this value being ignored.

         (U//FOUO,PR) If the hTPSAECN column is set to disabled(1), this
         value should be one octet in length: 0x00, 0x01, 0x02, 0x03.
         Any attempt to set this column to value outside of this range
         should, in this case, fail with an wrong value error.  Any
         bits set in these lower/right-most 2 of the octet will indicate
         that those bits be copied from the incoming packet's Explicit
         Congestion Notification (ECN) header value to the outgoing
         packet. Any bits set for this value effectively overrides, to
         copy, those set in the above altaTPSAECNValue column."
    DEFVAL { '00'h }
    ::= { altasecTPSAEntry 4 }

altasecTPSATTLHopLimit  OBJECT-TYPE
    SYNTAX              INTEGER (0..255)
    MAX-ACCESS          read-write
    STATUS              current
    DESCRIPTION
        "(U//FOUO,PR) The TTL or Hop Limit value for a packet associated with
         this SA. If this value has not been set when an internal SA goes
         active, it will be overridden with the current box-level default for
         the appropriate address type, from either the hIfv4TTL or hIfv6HopLimit
         objects in the HAIPE-NETWORKING-MIB.
         
         If this value is set to 0, the TTL or Hop Limit will be copied
         from the PT packet header into the CT packet header and
         decremented by one."
    ::= { altasecTPSAEntry 5 }

altasecTPSADFBitSetting  OBJECT-TYPE
    SYNTAX               INTEGER { set(1), clear(2), copy(3) }
    MAX-ACCESS           read-write
    STATUS               current
    DESCRIPTION
        "(U//FOUO,PR) The user setting of how to handle the DF bit created in
         the CT packet: set it; clear it, or copy the DF bit from the PT packet.

         (U//FOUO,PR) Note: This column is only pertinent to IPv4 traffic that
         is being encrypted (i.e. PT->CT traffic).

         (U//FOUO,PR) Note: This object is always writeable and contains the
         user setting to be applied should IPv4 Fixed Packet Length (FPL) be
         disabled and PT be the Preferred Fragmentation Side as represented by
         the altasecIfv4TunnelPreferredFragmentationSide object. If IPv4 FPL is
         enabled, only 'Clear' is a valid operational value. If IPv4 FPL is
         disabled and CT is the Preferred Fragmentation Side, only 'Copy' and
         'Clear' may become operational. A user setting of 'Set' will translate
         into an operational setting of 'Clear'. Otherwise (IPv4 FPL off & PT
         Frag Side), all three settings are operationally valid. To verify the
         current operational value for this object, see hTPSADFBit."
    DEFVAL { clear }
    ::= { altasecTPSAEntry 6 }

altasecTPSAName      OBJECT-TYPE
    SYNTAX           SnmpAdminString
    MAX-ACCESS       read-only
    STATUS           current
    DESCRIPTION
        "(U//FOUO,PR) The device name given to the internal SA device, logically
         grouping two or four entries in the table. If the SA is not currently
         active, it will not have a device name and return a zero length
         string. Device names themselves are unique to the logically linked
         SPI values they identify."
    ::= { altasecTPSAEntry 7 }

altasecTPSAIsMulticast  OBJECT-TYPE
    SYNTAX              TruthValue
    MAX-ACCESS          read-only
    STATUS              current
    DESCRIPTION
        "(U//FOUO,PR) This isMulticast flag is read-only and is provided as a
         convenience to the operator, as this option can be gathered from the IP
         address associated with this SA. If an address has not yet been
         specified, a false TruthValue will be returned."
    ::= { altasecTPSAEntry 8 }

altasecTPSAMulticastBypassMode  OBJECT-TYPE
    SYNTAX                      TruthValue
    MAX-ACCESS                  read-write
    STATUS                      current
    DESCRIPTION
        "(U//FOUO,PR) The Multicast Bypass/Host support method for this specific
          SA. This value is only available should the SA be multicast."
    DEFVAL { false }
    ::= { altasecTPSAEntry 9 }

altasecTPSAMulticastResetSource  OBJECT-TYPE
    SYNTAX                       TruthValue
    MAX-ACCESS                   read-write
    STATUS                       current
    DESCRIPTION
        "(U//FOUO,PR) Causes decrypted multicast traffic to have the HAIPE's PT
         IP address as its source address, instead of the multicast sender's
         original IP address."
    DEFVAL { false }
    ::= { altasecTPSAEntry 10 }

altasecTPSAConnectionStatus  OBJECT-TYPE
    SYNTAX                   INTEGER { unknown(0), up(1), down(2),
                                       noKey(3), noRoute(4), noPolicy(5),
                                       notReachable(6), needSuiteA(7),
                                       needSuiteB(8), invalidKey(9),
                                       noSPI(10), noGCM0(11) }
    MAX-ACCESS               read-write
    STATUS                   current
    DESCRIPTION
        "(U//FOUO,PR) The status of this SA with regards to its ability to
         pass traffic.

         (U//FOUO,PR) This object may be used to manually initiate HIKE, for
         HIKE SAs, by setting this object to 'up'. Similarly a connected HIKE SA
         can be disconnected by setting this object to 'down'. Any other setting
         will result in a wrongValue error. An attempt to modify this setting
         for an Inactive or non-HIKE SA will result in an inconsistentValue
         error.

         (U//FOUO,PR) If FPL is enabled, all Transport SAs for that respective
         interface (IPv4 and/or IPv6) will have a value of 'down', unless the
         Transport's Transform is not supported in the current suite."
    ::= { altasecTPSAEntry 11 }

altasecTPSATransformID       OBJECT-TYPE
    SYNTAX                   INTEGER { unknown(0),
                                       useGlobalTransforms(1),
                                       suiteAmedgcm96eff(2),
                                       suiteAmedgcm128eff(3),
                                       legacymedbip32eff(4),
                                       altasecmedbip32eff(5),
                                       altasecmedbip32bff(6),
                                       legacybatonbip32eff(7),
                                       legacymedbip32bff(8),
                                       legacybatonbip32bff(9),
                                       suiteBaesgcm96mqv(10),
                                       suiteBaesgcm128mqv(11),
                                       suiteAmedgcm96efftran(12),
                                       suiteAmedgcm0efftran(13),
                                       suiteAmedgcm128efftran(14),
                                       suiteBmedgcm96efftran(15),
                                       suiteBmedgcm0efftran(16),
                                       suiteBmedgcm128efftran(17)
                                     }
    MAX-ACCESS               read-write
    STATUS                   current
    DESCRIPTION
        "(U//FOUO,PR) When read, the transform used to protect the traffic as
         determined by the protecting PPK chain for PPK SAs or by the
         negotiated transform for HIKE SAs.
         
         (U//FOUO,PR) When written, only applicable for inactive manually
         configured HIKE SAs, the transform(s) that will be offered for
         negotiation. If 'useGlobalTransforms' is selected then the configured
         list in the hIPhFFSATransformsTable will be offered."
    DEFVAL { useGlobalTransforms }
    ::= { altasecTPSAEntry 12 }

altasecTPSATransportPacketsDropped             OBJECT-TYPE
    SYNTAX                                     Counter64
    MAX-ACCESS                                 read-only
    STATUS                                     current
    DESCRIPTION
        "(U//FOUO,PR) For Transport SAs, if the PT packet length is greater than
         the CT MTU then the packet will be dropped since fragmentation cannot
         occur. This object serves as a counter to indicate the number of
         packets that have been dropped on this SA (the grouped set of SA
         entries by altasecTPSAName) due to packet length since the last reboot.
         Tunnel SAs and non-active Transport SAs do not have this object."
    ::= { altasecTPSAEntry 13 }

altasecTPSATransportFragmentedPacketsDropped   OBJECT-TYPE
    SYNTAX                                     Counter64
    MAX-ACCESS                                 read-only
    STATUS                                     current
    DESCRIPTION
        "(U//FOUO,PR) For Transport SAs, packets that have already been
         fragmented on the PT network cannot be sent to the CT network. This
         object serves as a counter to indicate the number of packets that have
         been dropped on this SA (the grouped set of SA entries by
         altasecTPSAName) due to PT-side fragmentation since the last reboot.
         Tunnel SAs and non-active Transport SAs do not have this object."
    ::= { altasecTPSAEntry 14 }

--
-- Proprietary Global SA Settings
--

altasecESPv1PSEQNMultiplier   OBJECT-TYPE
    SYNTAX                    OCTET STRING (SIZE(8))
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) Global SA setting that allows the ESPv1 PSEQN multiplier
         to change to a user-desired value.  This value is a 64-bit unsigned
         integer represented here as an 8-octet string.  This serves as a
         straightforward way to meet requirements while still keeping
         operability with older AltaSec INEs."
        ::= { altasecTPSAManagementSettings 1 }

altasecESPv3PSEQNMultiplier   OBJECT-TYPE
    SYNTAX                    OCTET STRING (SIZE(8))
    MAX-ACCESS                read-write
    STATUS                    current
    DESCRIPTION
        "(U//FOUO,PR) Global SA setting that allows the ESPv3 PSEQN multiplier
         to change to a user-desired value.  This value is a 64-bit unsigned
         integer represented here as an 8-octet string.  This serves as a
         straightforward way to meet requirements while still keeping
         operability with older AltaSec INEs."
        ::= { altasecTPSAManagementSettings 2 }

-- ********************************************************************
-- altasecKTIBDisabledKeyTable
-- holds the list of keys disabled for in-band key transfersn

altasecKTIBDisabledKeyTableCount OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "(U//FOUO) The number of rows in the
        ALTASECKIBDisabledKeyTable."
     ::= { altasecKTIBDisabledKeyInformation 1 }

altasecKTIBDisabledKeyTableLastChanged OBJECT-TYPE
    SYNTAX      TimeStamp
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "(U//FOUO) The value of sysUpTime the last time any entry
         in the altasecKTIBDisabledKeyTable was modified, created, or
         deleted by either SNMP or other management method (.eg.,
         via an HMI).

         (U//FOUO) Managers can use this object to ensure that no
         changes to configuration of this table have happened since
         the last time it examined the table.

         (U//FOUO) A value of 0 indicates that no entry has been
         changed since the agent initialized."
     ::= { altasecKTIBDisabledKeyInformation 2 }

altasecKTIBDisabledKeyTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF ALTASECKTIBDisabledKeyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "(U//FOUO) The table for configuring a list of the client's
        keys for which in-band key transfer supersession is disabled.
        This table resides on the client. This proprietary table is necessary in
        that users may optionally load multiple copies of the same key. This
        table differs from the hKTIBDisabledKeyTable in that the PPK unique IDs are
        used as an additional index into the table."
    ::= { altasecKTIBDisabledKeyInformation 3 }

altasecKTIBDisabledKeyEntry OBJECT-TYPE
    SYNTAX      ALTASECKTIBDisabledKeyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "(U//FOUO) A row which identifies a key that is disabled for
        in-band key transfers."
    INDEX { altasecKTIBDKShortTitle, altasecKTIBDKPPKEdition,
            altasecKTIBDKPPKSegment, altasecKTIBDKPPKID }
    ::= { altasecKTIBDisabledKeyTable 1 }

ALTASECKTIBDisabledKeyEntry ::= SEQUENCE
    {
    altasecKTIBDKShortTitle          PPKShortTitle,
    altasecKTIBDKPPKEdition          PPKEdition,
    altasecKTIBDKPPKSegment          HaipeSegment,
    altasecKTIBDKPPKID               Unsigned32,
    altasecKTIBDKRowStatus           RowStatus
    }

-- changed to unique name (CHIMERA Team)
altasecKTIBDKShortTitle  OBJECT-TYPE
    SYNTAX               PPKShortTitle
    MAX-ACCESS           read-create
    STATUS               current
    DESCRIPTION
        "(U//FOUO) The short title of the disabled key."
    ::= { altasecKTIBDisabledKeyEntry 1 }

-- changed to unique name (CHIMERA Team)
altasecKTIBDKPPKEdition  OBJECT-TYPE
    SYNTAX               PPKEdition
    MAX-ACCESS           read-create
    STATUS               current
    DESCRIPTION
        "(U//FOUO) The edition of the disabled key."
    ::= { altasecKTIBDisabledKeyEntry 2 }

-- changed to unique name (CHIMERA Team)
altasecKTIBDKPPKSegment  OBJECT-TYPE
    SYNTAX               HaipeSegment
    MAX-ACCESS           read-create
    STATUS               current
    DESCRIPTION
        "(U//FOUO) The segment number of the disabled key."
    ::= { altasecKTIBDisabledKeyEntry 3 }

altasecKTIBDKPPKID  OBJECT-TYPE
    SYNTAX          Unsigned32
    MAX-ACCESS      read-create
    STATUS          current
    DESCRIPTION
            "(U//FOUO,PR) The unique ID assigned to the disabled key."
        ::= { altasecKTIBDisabledKeyEntry 4 }

-- changed to unique name (CHIMERA Team)
altasecKTIBDKRowStatus  OBJECT-TYPE
    SYNTAX              RowStatus
    MAX-ACCESS          read-create
    STATUS              current
    DESCRIPTION
        "(U//FOUO) The status of the row, by which new entries may
        be created, or old entries deleted from this table.

        (U//FOUO) The value of this object has no effect on whether
        other objects in this row can be modified.

        (U//FOUO) If any column is set to an invalid or unsupported
        value (during row creation or any other time), a wrongValue
        error will be returned for that object.

        (U//FOUO) An active row cannot be changed to an inconsistent
        state (an inconsistentValue error will be returned for the
        object)."
    ::= { altasecKTIBDisabledKeyEntry 5 }

END

--
--            UNCLASSIFIED//FOR OFFICIAL USE ONLY, PROPIN
--
