        SYNTAX            ALTASECTopologyEntry
        MAX-ACCESS        not-accessible
        STATUS            current
        DESCRIPTION
            "(U//FOUO,PR) A row defining the AltaSec proprietary extensions to
             the Topology Table providing explicit mapping between remote
             enclave entries and tunnel SAs."
        AUGMENTS   { hTopologyEntry }
        ::= { altasecTopologyTable 1 }

-- added missing columns (CHIMERA Team)
ALTASECTopologyEntry ::= SEQUENCE
    {
        altasecTopologyDestHostAddrType       InetAddressType,
        altasecTopologyDestHost               InetAddress,
        altasecTopologyDestHostPTPfxLen       InetAddressPrefixLength,
        altasecTopologyDestHaipeCTAddrType    InetAddressType,
        altasecTopologyDestHaipeCTAddr        InetAddress,
        altasecTopologyTunnelAssociationName  SnmpAdminString,
        altasecTopologyDestinationName        SnmpAdminString
    }

-- unofficial definition (CHIMERA Team)
altasecTopologyDestHostAddrType  OBJECT-TYPE
        SYNTAX                   InetAddressType
        MAX-ACCESS               read-only
        STATUS                   current
        DESCRIPTION
            "(U//FOUO) The PT Host Internet address type.

            (U//FOUO) Only ipv4(1), ipv6(2) and ipv6z(4) address types
            are supported."
        ::= { altasecTopologyEntry 1 }

-- unofficial definition (CHIMERA Team)
altasecTopologyDestHost  OBJECT-TYPE
        SYNTAX           InetAddress (SIZE (4|16|20))
        MAX-ACCESS       read-only
        STATUS           current
        DESCRIPTION
            "(U//FOUO) The PT Host (or network) IP
            address(es)."
        ::= { altasecTopologyEntry 2 }

-- unofficial definition (CHIMERA Team)
altasecTopologyDestHostPTPfxLen  OBJECT-TYPE
        SYNTAX                   InetAddressPrefixLength
        MAX-ACCESS               read-only
        STATUS                   current
        DESCRIPTION
            "(U//FOUO) The prefix length of the address
            stored in hTopologyDestHost (IE, the network mask)."
        ::= { altasecTopologyEntry 3 }

-- unofficial definition (CHIMERA Team)
altasecTopologyDestHaipeCTAddrType  OBJECT-TYPE
        SYNTAX                      InetAddressType
        MAX-ACCESS                  read-only
        STATUS                      current
        DESCRIPTION
            "(U//FOUO) The Destination HAIPE CT Internet
            address type.

            (U//FOUO) Only ipv4(1), ipv6(2) and ipv6z(4) address types
            are supported."
        ::= { altasecTopologyEntry 4 }

-- unofficial definition (CHIMERA Team)
altasecTopologyDestHaipeCTAddr  OBJECT-TYPE
        SYNTAX                  InetAddress (SIZE (4|16|20))
        MAX-ACCESS              read-only
        STATUS                  current
        DESCRIPTION
            "(U//FOUO) The Destination HAIPE CT IP address."
        ::= { altasecTopologyEntry 5 }

altasecTopologyTunnelAssociationName  OBJECT-TYPE
        SYNTAX                        SnmpAdminString
        MAX-ACCESS                    read-write
        STATUS                        current
        DESCRIPTION
            "(U//FOUO,PR) For remote enclave (PEPT) entries, the device name of
             the Tunnel that is currently associated with this entry. If the
             PEPT entry does not have a Tunnel association, a zero length
             string is returned. Local enclave (LEPT) entries do not have this
             object."
        DEFVAL { "" }
        ::= { altasecTopologyEntry 6 }

altasecTopologyDestinationName  OBJECT-TYPE
        SYNTAX                  SnmpAdminString
        MAX-ACCESS              read-write
        STATUS                  current
        DESCRIPTION
            "(U//FOUO,PR) For remote enclave (PEPT) entries, the destination 
             name of destination network. If the PEPT entry does not have a 
             destination name, a zero length string is returned. Local enclave 
             (LEPT) entries do not have this object."
        DEFVAL { "" }
        ::= { altasecTopologyEntry 7 }

-- *************************************************
