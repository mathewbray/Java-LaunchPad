-- *************************************************
-- Proprietary Signature Download.

altasecSoftwareSignatureDownloadAdminStatus OBJECT-TYPE
        SYNTAX          INTEGER
                        {
                           downloadSignature(1),
                           executeSignature(2),
                           deleteSignature(3),
                           allowSignatureDownloadAction(4)
                        }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Software Signature File Download Admin State. It
             mirrors the purpose of hFirmwareDownloadAdminStatus in the HAIPE
             Management MIB which is used to control Firmware Download."
        ::= { altasecMgmtSigDL 1 }

altasecSoftwareSignatureDownloadOperStatus  OBJECT-TYPE
        SYNTAX          INTEGER
                        {
                           noPendingOperation(0),
                           downloadInProgress(1),
                           downloadComplete(2),
                           downloadFailed(3),
                           executeInProgress(4),
                           executeComplete(5),
                           executeFailed(6),
                           deleteInProgress(7),
                           deleteComplete(8),
                           deleteFailed(9)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Software Signature File Download Operational State. It
             mirrors the purpose of hFirmwareDownloadOperStatus in the HAIPE
             Management MIB which is used to check the status of a Firmware
             Download."
        ::= { altasecMgmtSigDL 2 }

altasecSoftwareSignatureDownloadResidentId OBJECT-TYPE
        SYNTAX          OCTET STRING (SIZE(1..256))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Software Signature currently loaded."
        ::= { altasecMgmtSigDL 3 }

-- *************************************************
-- Proprietary Battery Change Date.

altasecBatteryTable     OBJECT-TYPE
    SYNTAX              SEQUENCE OF ALTASECBatteryEntry
    MAX-ACCESS          not-accessible
    STATUS              current
    DESCRIPTION
        "(U//FOUO,PR) The table containing a writable change date of the
         external battery.  It augments the hBatteryTable in the
         HAIPE-MANAGEMENT-MIB."
    ::= { altasecMgmtBattery 3 }

altasecBatteryEntry OBJECT-TYPE
    SYNTAX      ALTASECBatteryEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "(U//FOUO,PR) A row describing a particular battery change date within
         the HAIPE."
    AUGMENTS { hBatteryEntry }
    ::= { altasecBatteryTable 1 }

-- added missing column (CHIMERA Team)
ALTASECBatteryEntry ::= SEQUENCE
    {
        altasecBatteryNumber             Unsigned32,
        altasecBatteryChangeDate         DateAndTime
    }

-- unofficial definition (CHIMERA Team)
altasecBatteryNumber    OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) The unique number enumerating a given battery."
        ::= { altasecBatteryEntry 1 }

altasecBatteryChangeDate     OBJECT-TYPE
        SYNTAX               DateAndTime
        MAX-ACCESS           read-write
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) The date and time a changeable battery was last
             replaced. If a battery is not changeable, this column should be
             not-accessible. If a battery is changeable, but has not been
             changed or the last change time was lost then this value should
             be composed with all 0 values.

             (U//FOUO,PR) A user may set to this object, specifying a date
             within the past year or up to 7 days in the future."
        ::= { altasecBatteryEntry 2 }

-- *************************************************
-- Networking Settings.

-- unofficial definition (CHIMERA Team)
altasecNetErrorOID      OBJECT-TYPE
        SYNTAX          OBJECT IDENTIFIER
        MAX-ACCESS      accessible-for-notify
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) OID of the object linked to the error string caused by
             a set operation. If the column is not known, 0 is returned for the
             column OID. If the object is not known then a ZeroDotZero value
             (0.0) is returned."
        ::= { altasecNetObjects 1 }

-- unofficial definition (CHIMERA Team)
altasecNetErrorString   OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      accessible-for-notify
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Error string caused by a set operation."
        ::= { altasecNetObjects 2 }

altasecNetRebootRequired        NOTIFICATION-TYPE
        OBJECTS         { hStationID, hrSystemDate }
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) The notification from HAIPE device to HMS indicating
             that a reboot is required to apply the latest configuration
             changes."
        ::= { altasecNetNotifications 1 }

-- unofficial definition (CHIMERA Team)
altasecNetSetError      NOTIFICATION-TYPE
        OBJECTS         { hStationID, hrSystemDate, altasecNetErrorOID,
                          altasecNetErrorString }
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) An SNMP set has caused an internal error associated
             with a textual display string."
        ::= { altasecNetNotifications 3 }

