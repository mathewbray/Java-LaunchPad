-- *************************************************
-- Proprietary IPv4 interface augmentation table.

altasecIfv4TableAugmentation OBJECT IDENTIFIER ::= { altasecNetIPv4 2 }

altasecIfv4HaipeTable   OBJECT-TYPE
    SYNTAX              SEQUENCE OF ALTASECIfv4HaipeEntry
    MAX-ACCESS          not-accessible
    STATUS              current
    DESCRIPTION
        "(U//FOUO,PR) The table containing a list of AltaSec-specific interface
         objects for IPv4.  It augments the hIfv4HaipeEntry in the
         HAIPE-NETWORKING-MIB."
    ::= { altasecIfv4TableAugmentation 3 }

altasecIfv4HaipeEntry OBJECT-TYPE
    SYNTAX      ALTASECIfv4HaipeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "(U//FOUO,PR) A row mapping IPv4 interface variables to an interface."
    AUGMENTS { ipv4InterfaceEntry }
    ::= { altasecIfv4HaipeTable 1 }

ALTASECIfv4HaipeEntry ::= SEQUENCE
    {
        altasecIfv4ICMPAllowPing                            TruthValue,
        altasecIfv4ICMPProcessPMTU                          TruthValue,
        altasecIfv4ICMPProcessPMTUSetting                   TruthValue,
        altasecIfv4DHCP                                     TruthValue,
        altasecIfv4ARPCacheTimeout                          Unsigned32,
        altasecIfv4ProxyARP                                 TruthValue
    }

altasecIfv4ICMPAllowPing     OBJECT-TYPE
        SYNTAX               TruthValue
        MAX-ACCESS           read-write
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) Allows IPv4 Informational ICMP messages to be
             processed by the Linux kernel, this includes echo responses to
             incoming echo requests. This is true(1) by default for the PT
             interface and false(2) by default for the CT interface."
        ::= { altasecIfv4HaipeEntry 1 }

altasecIfv4ICMPProcessPMTU   OBJECT-TYPE
        SYNTAX               TruthValue
        MAX-ACCESS           read-only
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) Allows IPv4 Error ICMP messages (such as PMTU
             Discovery, and Source Quench) to be processed by the Linux kernel,
             these are then bypassed out to the PT network after being handled
             internally.

             (U//FOUO,PR) For the CT interface, if IPv4 Fixed Packet Length
             (FPL) is enabled, then this object is internally forced to False.
             To read and write to its user setting see the
             altasecIfv4ICMPProcessPMTUSetting object. If FPL is currently
             enabled, the user setting will be utilized once FPL is disabled.
             The PT interface is not affected by such behavior and its value
             will mirror that of the altasecIfv4ICMPProcessPMTUSetting object."
        ::= { altasecIfv4HaipeEntry 2 }

altasecIfv4ICMPProcessPMTUSetting    OBJECT-TYPE
        SYNTAX                       TruthValue
        MAX-ACCESS                   read-write
        STATUS                       current
        DESCRIPTION
            "(U//FOUO,PR) Allows IPv4 Error ICMP messages (such as PMTU
             Discovery, and Source Quench) to be processed by the Linux kernel,
             these are then bypassed out to the PT network after being handled
             internally. This is true(1) by default for the PT  interface and
             false(2) by default for the CT interface.

             (U//FOUO,PR) Note: This object is always writeable and contains,
             for the CT interface, the user setting to be applied should IPv4
             Fixed Packet Length (FPL) be disabled. To verify the current
             operational value for this object see the
             altasecIfv4ICMPProcessPMTU object."
        DEFVAL { false }
        ::= { altasecIfv4HaipeEntry 3 }

altasecIfv4DHCP              OBJECT-TYPE
        SYNTAX               TruthValue
        MAX-ACCESS           read-write
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) Interface DHCP enable Truth Value."
        DEFVAL { false }
        ::= { altasecIfv4HaipeEntry 4 }

altasecIfv4ARPCacheTimeout   OBJECT-TYPE
        SYNTAX               Unsigned32
        MAX-ACCESS           read-write
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) ARP cache timeout in seconds."
        DEFVAL { 300 }
        ::= { altasecIfv4HaipeEntry 5 }

altasecIfv4ProxyARP          OBJECT-TYPE
        SYNTAX               TruthValue
        MAX-ACCESS           read-write
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) Proxy ARP allows the PT (Red) interface to respond to
             ARP requests for hosts connected by a security association.
             Note: This only applies to the PT interface. Values set to the CT
             interface will return a wrongValue error."
        DEFVAL { false }
        ::= { altasecIfv4HaipeEntry 6 }


