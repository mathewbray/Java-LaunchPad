--
--            UNCLASSIFIED//FOR OFFICIAL USE ONLY, PROPIN
--
-- This entire document, which defines AltaSec Proprietary MIB objects in the
-- SMIv2 language should be considered U//FOUO,PR.
--
-- The data that will be transmitted on the wire as a result of
-- accessing these MIB objects MUST NOT BE CONSIDERED U//FOUO and is
-- subject to separate classification.  The classification level of this
-- data is not defined or within the scope of this document.
--

ALTASEC-MIB DEFINITIONS ::= BEGIN

IMPORTS
    OBJECT-TYPE, NOTIFICATION-TYPE,
    enterprises, Integer32, Unsigned32,
    Counter64, MODULE-IDENTITY            FROM SNMPv2-SMI
    RowStatus, DateAndTime, TruthValue,
    TimeStamp, DisplayString              FROM SNMPv2-TC
    SnmpAdminString                       FROM SNMP-FRAMEWORK-MIB
    InterfaceIndex, ifEntry               FROM IF-MIB
    InetAddressType, InetAddress,
    InetAddressPrefixLength               FROM INET-ADDRESS-MIB
    hrSystemDate                          FROM HOST-RESOURCES-MIB
    HaipeSecurityLevel,
    HaipeSecurityAuthority,
    HaipeSide                             FROM HAIPE-TC-MIB
    hStationID, hBatteryEntry             FROM HAIPE-MANAGEMENT-MIB
    ipv4InterfaceEntry,
    ipv6InterfaceEntry                    FROM IP-MIB
    PPKShortTitle,
    PPKEdition,
    HaipeSegment,
    hIPhRuleEntry                         FROM HAIPE-TRAFFIC-PROTECTION-MIB
    hTopologyEntry,
    hRIPConfEntry                         FROM HAIPE-NETWORKING-DISCOVERY-MIB
    hPPKChainEntry,
    HaipeIntegrityAlgorithm,
    hIPhFFSAEntry, hTPSAEntry             FROM HAIPE-TRAFFIC-PROTECTION-MIB
    usmUserSecurityName                   FROM SNMP-USER-BASED-SM-MIB;


altasecMIB MODULE-IDENTITY
    LAST-UPDATED "200912100000Z"
    ORGANIZATION "ViaSat"
    CONTACT-INFO
         "(U//FOUO) Not Available"
    DESCRIPTION
        "(U//FOUO) This MIB defines the AltaSec Proprietary MIB objects."
    REVISION     "200912100000Z"
    DESCRIPTION
        "(U//FOUO,PR) AltaSec Proprietary MIB objects for HAIPE 3.1.2.
         For internal 3.1.7 build."
    REVISION     "200910210000Z"
    DESCRIPTION
        "(U//FOUO,PR) First draft AltaSec Proprietary MIB objects for HAIPE
         3.1.2."
    REVISION     "200805140000Z"
    DESCRIPTION
        "(U//FOUO,PR) AltaSec Proprietary MIB objects for HAIPE 3.0.2."
    ::= { viaSat 3 }

viaSat OBJECT IDENTIFIER ::= { enterprises 8698 }

--
-- AltaSec Information Segments
