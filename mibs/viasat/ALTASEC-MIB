-- *************************************************
-- Remote Management.

-- added old syntax values to description (CHIMERA Team)
altasecMgmtRemoteSetting    OBJECT-TYPE
        SYNTAX          INTEGER
                        {
                           disabled(0),
                           everyone(1),
                           ipTable(2)
                        }
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) The setting for how remote management should be
             handled. disabled indicates that remote management is disabled.
             everyone indicates that everyone is able to remotely manage
             this device. ipTable indicates that the 
             altasecMgmtRemoteAddressTable is used to filter who is remotely
             able to manage this device.

             (U//FOUO,PR) Old values: enabled(0), disabled(1), single(2)."
        DEFVAL { disabled }
        ::= { altasecMgmtRemote 1 }

altasecMgmtRemoteAddress4 OBJECT-TYPE
        SYNTAX           InetAddress
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) Remote Management IPv4 address."
        ::= { altasecMgmtRemote 2 }

altasecMgmtRemoteAddress6 OBJECT-TYPE
        SYNTAX           InetAddress
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) Remote Management IPv6 address."
        ::= { altasecMgmtRemote 3 }

altasecMgmtUDPZeroizeInterval  OBJECT-TYPE
        SYNTAX           Unsigned32
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) UDP Management interval before a zeroize executes."
        DEFVAL { 50 }
        ::= { altasecMgmtRemote 4 }

altasecMgmtUDPZeroize    OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) UDP Management zeroize flag."
        ::= { altasecMgmtRemote 5 }

altasecMgmtUDPPort OBJECT-TYPE
        SYNTAX           Unsigned32
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) UDP Management IP port."
        DEFVAL { 3000 }
        ::= { altasecMgmtRemote 6 }

altasecMgmtUDPRememberHbt OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) UDP Management Remember Heartbeat flag. Setting this
            flag causes the INE to remember if a Heartbeat has been previously
            received across reboots."
        ::= { altasecMgmtRemote 7 }

-- *************************************************
-- Remote Management Address Table.

altasecMgmtRemoteAddressTableCount       OBJECT-TYPE
        SYNTAX              Unsigned32
        MAX-ACCESS          read-only
        STATUS              current
        DESCRIPTION
            "(U//FOUO,PR) The number of rows in the altasecMgmtRemoteAddressTable."
        ::= { altasecMgmtRemote 8 }

-- removed conflicting OID assignment (CHIMERA Team)
-- altasecMgmtRemoteAddressTableTableLastChanged OBJECT-TYPE
--         SYNTAX              TimeStamp
--         MAX-ACCESS          read-only
--         STATUS              current
--         DESCRIPTION
--             "(U//FOUO,PR) The value of sysUpTime the last time any entry in
--              the altasecMgmtRemoteAddressTable was modified, created, or deleted by
--              either SNMP or other management method (e.g., via an HMI).
-- 
--              (U//FOUO,PR) Managers can use this object to ensure that no
--              changes to configuration of this table have happened since the
--              last time it examined the table.
-- 
--              (U//FOUO,PR) A value of 0 indicates that no entry has been
--              changed since the agent initialized."
--         ::= { altasecMgmtRemote 9 }

altasecMgmtRemoteAddressTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF ALTASECMgmtRemoteAddressEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) A table containing a list of networks that are 
             allowed to remotely manage this HAIPE. Note that networks 
             must be distinct (i.e. An Entry within this table cannot be 
             a sub net of another entry)."
        ::= { altasecMgmtRemote 9 }

altasecMgmtRemoteAddressEntry OBJECT-TYPE
        SYNTAX          ALTASECMgmtRemoteAddressEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) A row defining the IP configuration for a remote management
             network."
        INDEX   { altasecMgmtRemoteAddressType,
                  altasecMgmtRemoteAddressStart }
        ::= { altasecMgmtRemoteAddressTable 1 }

ALTASECMgmtRemoteAddressEntry ::= SEQUENCE
    {
        altasecMgmtRemoteAddressType            InetAddressType,
        altasecMgmtRemoteAddressStart           InetAddress,
        altasecMgmtRemoteAddressPrefixLength    Unsigned32,
        altasecMgmtRemoteAddressLabel           OCTET STRING,
        altasecMgmtRemoteAddressRowStatus       RowStatus
    }

altasecMgmtRemoteAddressType     OBJECT-TYPE
        SYNTAX             InetAddressType
        MAX-ACCESS         read-create
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The Internet address type for this remote management 
             network."
        DEFVAL { ipv4 }
        ::= { altasecMgmtRemoteAddressEntry 1 }

altasecMgmtRemoteAddressStart         OBJECT-TYPE
        SYNTAX             InetAddress (SIZE (4|16))
        MAX-ACCESS         read-create
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The starting IP address for the remote management 
             network."
        ::= { altasecMgmtRemoteAddressEntry 2 }
        
altasecMgmtRemoteAddressPrefixLength         OBJECT-TYPE
        SYNTAX             Unsigned32 (0..128)
        MAX-ACCESS         read-create
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) The prefix length for this remote network."
        ::= { altasecMgmtRemoteAddressEntry 3 }

altasecMgmtRemoteAddressLabel              OBJECT-TYPE
        SYNTAX             OCTET STRING (SIZE(1..32))
        MAX-ACCESS         read-create
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) An administratively assigned label identifying 
             this entry."
        ::= { altasecMgmtRemoteAddressEntry 4 }
        
altasecMgmtRemoteAddressRowStatus         OBJECT-TYPE
        SYNTAX             RowStatus
        MAX-ACCESS         read-create
        STATUS             current
        DESCRIPTION
            "(U//FOUO,PR) Row status for the Remote Management entry."
        ::= { altasecMgmtRemoteAddressEntry 5 }

altasecMgmtClearAlerts OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) This object will always return false(2). Setting this
             object to true(1) will clear the alert Log."
        ::= { altasecMgmtAlert 1 }

altasecMgmtRebootRequired OBJECT-TYPE
        SYNTAX            TruthValue
        MAX-ACCESS        read-only
        STATUS            current
        DESCRIPTION
            "(U//FOUO,PR) Flag indicating if configuration settings have changed
             which require a reboot to take effect."
        ::= { altasecMgmtAlert 2 }

altasecAuditLogCompression OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Audit Log Compression enabled flag."
        ::= { altasecMgmtAudit 1 }

altasecAuditLogAdminStatus OBJECT-TYPE
        SYNTAX          INTEGER
                        {
                           allowAction(0),
                           retrieve(1),
                           delete(2)
                        }
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) This scalar controls the audit log handling process.
             When read, it will return allowAction if the last action succeeded,
             or the last requested action, if an action failed or is still in
             progress."
        ::= { altasecMgmtAudit 2 }

altasecAuditLogOperStatus  OBJECT-TYPE
        SYNTAX          INTEGER
                        {
                           noPendingOperation(0),
                           retrievalInProgress(1),
                           retrievalComplete(2),
                           deletionInProgress(3),
                           deletionComplete(4),
                           retrievalFailed(5),
                           deletionFailed(6)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
            "(U//FOUO) This scalar indicates the current state of the audit log.
             retrievalInProgress(1) indicates that an audit log retrieval is
             underway. retrievalComplete(2) indicates that the most recent audit
             log has been successfully retrieved. deletionInProgress(3)
             indicates that an audit log deletion is underway.
             deletionComplete(4) indicates that the audit log has been deleted
             successfully. retrievalFailed(5) indicates that the retrieval of
             the audit log failed. deletionFailed(6) indicates that the deletion
             of the audit log failed."
        ::= { altasecMgmtAudit 3 }

altasecMgmtSendTrap OBJECT-TYPE
        SYNTAX          INTEGER
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Trigger INE to send an SNMP Trap, not accessible by
             users."
        DEFVAL { 0 }
        ::= { altasecMgmtTrap 1 }

altasecMgmtSendPassExpTrap OBJECT-TYPE
        SYNTAX          OCTET STRING (SIZE(1..32))
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Trigger INE to send an SNMP Trap indicating a user's
             password has expired, not accessible by users."
        ::= { altasecMgmtTrap 2 }

altasecMgmtErrorOID     OBJECT-TYPE
        SYNTAX          OBJECT IDENTIFIER
        MAX-ACCESS      accessible-for-notify
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) OID of the object linked to the error string caused by
             a set operation. If the column is not known, 0 is returned for the
             column OID. If the object is not known then a ZeroDotZero value
             (0.0) is returned."
        ::= { altasecMgmtTrapObjects 1 }

altasecMgmtErrorString  OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      accessible-for-notify
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Error string caused by a set operation."
        ::= { altasecMgmtTrapObjects 2 }

altasecMgmtPasswordExpired NOTIFICATION-TYPE
        OBJECTS         { hStationID, hrSystemDate, usmUserSecurityName }
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) The notification from HAIPE device to HMS indicating
             that a user's password has expired."
        ::= { altasecMgmtTrapNotifications 1 }

altasecMgmtSetError     NOTIFICATION-TYPE
        OBJECTS         { hStationID, hrSystemDate, altasecMgmtErrorString }
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) An SNMP set has caused an internal error associated
             with a textual display string."
        ::= { altasecMgmtTrapNotifications 2 }

altasecMgmtMaxSessions OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) The maximum allowed open sessions.  0 is unlimited."
        ::= { altasecMgmtSession 1 }

altasecMgmtOneSessionPerUser OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Only allow one open session per user."
        ::= { altasecMgmtSession 2 }

altasecMgmtInactiveSeconds OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
            "(U//FOUO,PR) Inactivity session timeout in seconds."
        ::= { altasecMgmtSession 3 }

altasecMgmtSuiteOfOperation  OBJECT-TYPE
        SYNTAX               INTEGER { unknown(0),
                                       suiteA(1),
                                       suiteB(2)
                                     }
        MAX-ACCESS           read-only
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) The current Suite of Operation. If an operational CIK
             is not inserted, the value of this object is unknown(0)."
        ::= { altasecMgmtCIK 1 }
        
altasecMgmtFTRCount          OBJECT-TYPE
        SYNTAX               Unsigned32
        MAX-ACCESS           read-only
        STATUS               current
        DESCRIPTION
            "(U//FOUO,PR) The number of times the INE has been recovered
             in the field. After the maximum count has been reached the unit
             must be returned to the factory for recovery from a tamper state."
        ::= { altasecMgmtCIK 2 }

