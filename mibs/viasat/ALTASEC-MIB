-- *************************************************
-- IPv4/IPv6 Box level settings.

altasecIPv4Settings OBJECT IDENTIFIER ::= { altasecNetIPv4 1 }
altasecIPv6Settings OBJECT IDENTIFIER ::= { altasecNetIPv6 1 }


altasecIPMulticastEnable OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) Global multicast enable Truth Value."
        ::= { altasecNetIPSettings 1 }

altasecIPReplayWindowSize OBJECT-TYPE
        SYNTAX            Unsigned32(32..32768)
        MAX-ACCESS        read-write
        STATUS            current
        DESCRIPTION
            "(U//FOUO,PR) The window size, in packets, that are used for
             anti-replay integrity, which detects arrival of duplicate IP
             datagrams (within a constrained window). In anti-replay integrity a
             packet's unique sequence number is compared to that of the
             previous packets (whose number to examine is defined by this value)
             on arrival.

             (U//FOUO,PR) This value may be 32, 64, 256, 1024, 4096, 8192,
             16384, or 32768. Note that in this progression 128, 512, and 2048
             are not supported."
        ::= { altasecNetIPSettings 2 }

altasecIPMulticastIGMPQueryEnable OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) IGMP query multicast enable Truth Value."
        ::= { altasecIPv4Settings 1 }

altasecIPMulticastIGMPVersion OBJECT-TYPE
        SYNTAX           INTEGER { none(0),
                                   version1(1),
                                   version2(2),
                                   version3(3)
                                 }
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) IGMP multicast version."
        ::= { altasecIPv4Settings 2 }

altasecIPMulticastMLDQueryEnable OBJECT-TYPE
        SYNTAX           TruthValue
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) MLD Query multicast enable Truth Value."
        ::= { altasecIPv6Settings 1 }

altasecIPMulticastMLDVersion OBJECT-TYPE
        SYNTAX           INTEGER { none(0), version1(1), version2(2) }
        MAX-ACCESS       read-write
        STATUS           current
        DESCRIPTION
            "(U//FOUO,PR) MLD multicast version."
        ::= { altasecIPv6Settings 2 }

