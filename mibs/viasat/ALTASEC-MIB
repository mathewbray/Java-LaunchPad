--

altasecIPhRuleTable   OBJECT-TYPE
    SYNTAX            SEQUENCE OF ALTASECIPhRuleEntry
    MAX-ACCESS        not-accessible
    STATUS            current
    DESCRIPTION
        "(U//FOUO,PR) The table for configuring AltaSec proprietary extensions
         to the HAIPE-TRAFFIC-PROTECTION-MIB's hIPhRuleTable."
    ::= { altasecTPiphRule 3 }

altasecIPhRuleEntry   OBJECT-TYPE
    SYNTAX            ALTASECIPhRuleEntry
    MAX-ACCESS        not-accessible
    STATUS            current
    DESCRIPTION
        "(U//FOUO) A row which describes the handling of IP
         messages arriving on a particular interface."
    AUGMENTS   { hIPhRuleEntry }
    ::= { altasecIPhRuleTable 1 }

ALTASECIPhRuleEntry ::= SEQUENCE
    {
        altasecIPhRuleActionProtectType              INTEGER,
        altasecIPhRuleActionProtectTransportSAIndex  Unsigned32,
        altasecIPhRuleActionProtectTransportName     SnmpAdminString
    }

altasecIPhRuleActionProtectType    OBJECT-TYPE
    SYNTAX                         INTEGER { tunnel(1), transport(2) }
    MAX-ACCESS                     read-write
    STATUS                         current
    DESCRIPTION
        "(U//FOUO,PR) The type of SA to protect should the parent hIPhRuleAction
         object be set to protectWithPPK or protectWithFF, else no object will
         be returned."
    DEFVAL { tunnel }
    ::= { altasecIPhRuleEntry 4 }

altasecIPhRuleActionProtectTransportSAIndex    OBJECT-TYPE
    SYNTAX                                     Unsigned32
    MAX-ACCESS                                 read-write
    STATUS                                     current
    DESCRIPTION
        "(U//FOUO,PR) The hTPSAIndex value to explicitly link a protect
         transport rule to a transport SA. If this value is zero, then a protect
         transport rule link is attempted on peer HAIPE and both host addresses.
         This may be needed if multiple Transports are configured between the
         same two Source and Destination Addresses. If the rule is not intended
         to protect transport SAs no object will be returned."
    DEFVAL { 0 }
    ::= { altasecIPhRuleEntry 5 }

altasecIPhRuleActionProtectTransportName       OBJECT-TYPE
    SYNTAX                                     SnmpAdminString
    MAX-ACCESS                                 read-only
    STATUS                                     current
    DESCRIPTION
        "(U//FOUO,PR) The device name given to the internal SA device, if
         configured to protect an active Transport SA. If the Transport SA is
         not currently active, it will not have a device name and return a zero
         length string. Other Rule types will not return this object."
    ::= { altasecIPhRuleEntry 6 }

