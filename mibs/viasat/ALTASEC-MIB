altasecIf           OBJECT IDENTIFIER ::= { altasecNetIP 2 }

-- *************************************************
-- Proprietary If augmentation table.

altasecIfTableAugmentation OBJECT IDENTIFIER ::= { altasecIf 2 }

altasecIfTable   OBJECT-TYPE
    SYNTAX       SEQUENCE OF ALTASECIfEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
        "(U//FOUO,PR) The table containing a list of AltaSec-specific interface
         objects.  It augments the ifEntry in the IF-MIB."
    ::= { altasecIfTableAugmentation 3 }

altasecIfEntry OBJECT-TYPE
    SYNTAX      ALTASECIfEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "(U//FOUO) A row mapping IPv6 interface variables to an interface."
    AUGMENTS { ifEntry }
    ::= { altasecIfTable 1 }

ALTASECIfEntry ::= SEQUENCE
    {
        altasecIfIndex                   InterfaceIndex,
        altasecIfDuplex                  INTEGER,
        altasecIfMode                    INTEGER,
        altasecIfNegotiateSpeedDuplex    BITS,
        altasecIfForceSpeed              Unsigned32,
        altasecIfForceDuplex             INTEGER
    }

-- changed to unique name (CHIMERA Team)
altasecIfIndex                   OBJECT-TYPE
        SYNTAX                   InterfaceIndex
        MAX-ACCESS               read-only
        STATUS                   current
        DESCRIPTION
            "A unique value, greater than zero, for each interface.  It
            is recommended that values are assigned contiguously
            starting from 1.  The value for each interface sub-layer
            must remain constant at least from one re-initialization of
            the entity's network management system to the next re-
            initialization."
    ::= { altasecIfEntry 1 }

altasecIfDuplex                  OBJECT-TYPE
        SYNTAX                   INTEGER { unknown(0),
                                           halfDuplex(1),
                                           fullDuplex(2)
                                         }
        MAX-ACCESS               read-only
        STATUS                   current
        DESCRIPTION
            "(U//FOUO, PR) Operational duplex setting of the current link."
        ::= { altasecIfEntry 2 }

altasecIfMode   OBJECT-TYPE
        SYNTAX                   INTEGER { autoNegotiate(1), force(2) }
        MAX-ACCESS               read-write
        STATUS                   current
        DESCRIPTION
            "(U//FOUO, PR) The mode by which link speed and duplex should be
             determined."
        DEFVAL { autoNegotiate }
        ::= { altasecIfEntry 3 }

-- added unofficial values 4-5 (CHIMERA Team)
altasecIfNegotiateSpeedDuplex    OBJECT-TYPE
        SYNTAX                   BITS { speed10MbitHalfDuplex(0),
                                        speed10MbitFullDuplex(1),
                                        speed100MbitHalfDuplex(2),
                                        speed100MbitFullDuplex(3),
                                        speed1000MbitHalfDuplex(4),
                                        speed1000MbitFullDuplex(5)
                                      }
        MAX-ACCESS               read-write
        STATUS                   current
        DESCRIPTION
            "(U//FOUO, PR) The speed and duplex modes that should be offered for
             negotiation. These settings are utilized when the altasecIfMode
             object is set to autoNegotiate(1)."
        DEFVAL { { speed10MbitHalfDuplex, speed10MbitFullDuplex,
                   speed100MbitHalfDuplex, speed100MbitFullDuplex } }
        ::= { altasecIfEntry 4 }

altasecIfForceSpeed              OBJECT-TYPE
        SYNTAX                   Unsigned32
        MAX-ACCESS               read-write
        STATUS                   current
        DESCRIPTION
            "(U//FOUO, PR) The speed mode that should be forced. This setting
             is utilized when the altasecIfMode object is set to force(2)."
        DEFVAL { 100000000 }
        ::= { altasecIfEntry 5 }

altasecIfForceDuplex             OBJECT-TYPE
        SYNTAX                   INTEGER { unknown(0),
                                           halfDuplex(1),
                                           fullDuplex(2)
                                         }
        MAX-ACCESS               read-write
        STATUS                   current
        DESCRIPTION
            "(U//FOUO, PR) The duplex mode that should be forced. This setting
             is utilized when the altasecIfMode object is set to force(2)."
        DEFVAL { fullDuplex }
        ::= { altasecIfEntry 6 }

